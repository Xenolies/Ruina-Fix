#. ID 1, Page 2, Line 5, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「喂，\\N[6]！\n"
"\\N[27]　你跑到哪闲晃去了！」"
msgstr ""

#. ID 1, Page 2, Line 9, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「哼哼哼。妈妈，说这种话真的好吗？\n"
"\\N[27]　你看，人家把客人带过来喽」"
msgstr ""

#. ID 1, Page 2, Line 14, Pos (7,10)
#. ID 1, Page 2, Line 20, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「什么客人啊，\n"
"\\N[27]　不就是\\N[\\V[2]]嘛……\n"
"\\N[27]　那，今天想看看什么？」"
msgstr ""

#. ID 1, Page 2, Line 31, Pos (7,10)
#. ID 1, Page 3, Line 11, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「啊，欢迎光临，\\N[\\V[2]]。\n"
"\\N[27]　今天是人家看店的日子`」"
msgstr ""

#. ID 1, Page 2, Line 40, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「来来，欢迎光临！　\n"
"\\N[27]　…哎呀，这不是\\N[\\V[2]]大人嘛。\n"
"\\N[27]　您今天有何贵干？」"
msgstr ""

#. ID 1, Page 2, Line 47, Pos (7,10)
#. ID 1, Page 2, Line 53, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「来来，欢迎光临！　\n"
"\\N[27]　…哎呀，这不是\\N[\\V[2]]嘛。\n"
"\\N[27]　今天有什么事啊？」"
msgstr ""

#. ID 1, Page 3, Line 29, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「……那座空中都城\n"
"\\N[27]　我有在梦里见过……」"
msgstr ""

#. ID 1, Page 3, Line 35, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「最近我经常做些奇怪的梦。\n"
"\\N[27]　像是在镇上迷路一直走个不停，\n"
"\\N[27]　是不是因为最近太过操劳的缘故？」"
msgstr ""

#. ID 1, Page 3, Line 42, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「原来\\N[130]大人也读过『废都物语』。\n"
"\\N[27]　你看，阿尔凯亚帝国就是在那小\n"
"\\N[27]　说里面出现过的国家吧？」"
msgstr ""

#. ID 1, Page 3, Line 49, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「哈啊……\n"
"\\N[27]　虽然总算性命无忧，\n"
"\\N[27]　店也算是保下来了……」"
msgstr ""

#. ID 1, Page 3, Line 53, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「但商品都被抢走，损失了一大笔呢。\n"
"\\N[27]　而且镇上变成这样也做不了生意了……」"
msgstr ""

#. ID 1, Page 3, Line 59, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「…………」"
msgstr ""

#. ID 1, Page 3, Line 61, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「哎呀，抱歉。\n"
"\\N[27]　刚刚我在读『废都物语』这本小说呢」"
msgstr ""

#. ID 1, Page 3, Line 67, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「河岸边建起了好几座巨大的锻造工房。\n"
"\\N[27]　这下河水肯定要被污染了」"
msgstr ""

#. ID 1, Page 3, Line 70, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「既然是\\N[130]大人的意思，\n"
"\\N[27]　我们也就只能保持沉默了」"
msgstr ""

#. ID 1, Page 3, Line 78, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「妮露！\n"
"\\N[27]　你还活着吗……！」"
msgstr ""

#. ID 1, Page 3, Line 82, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「真、真夸张啊，妈妈。\n"
"\\N[27]　别哭啊」"
msgstr ""

#. ID 1, Page 3, Line 88, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「\\N[\\V[2]]也在啊，\n"
"\\N[27]　欢迎你们回来。\n"
"\\N[27]　全都活得好好的呢」"
msgstr ""

#. ID 1, Page 3, Line 95, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「\\N[\\V[2]]，欢迎回来。\n"
"\\N[27]　你能平安真是太好了\n"
"\\N[27]　我女儿肯定也会高兴的」"
msgstr ""

#. ID 1, Page 3, Line 104, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「我从客人那听说，\n"
"\\N[27]　好象有人在遗迹深处见过真正的妖精呢」"
msgstr ""

#. ID 1, Page 3, Line 110, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「听说都市里的王子大人\n"
"\\N[27]　到领主大人那里来了。\n"
"\\N[27]　人家也好想见他一面啊`」"
msgstr ""

#. ID 1, Page 3, Line 116, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「也不想想自己多大岁数了」"
msgstr ""

#. ID 1, Page 3, Line 123, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「北边的森林好象出大事了呢。\n"
"\\N[27]　想知道详情就去领主\n"
"\\N[27]　大人那里打听一下吧」"
msgstr ""

#. ID 1, Page 3, Line 129, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「事情真的变得很严重了啊……　\n"
"\\N[27]　我们今后会变成怎样呢」"
msgstr ""

#. ID 1, Page 4, Line 8, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「DD啊，\\N[\\V[2]]。\n"
"\\N[27]　欢迎光临。\n"
"\\N[27]　今天有什么事吗」"
msgstr ""

#. ID 1, Page 4, Line 12, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「诶、人家？\n"
"\\N[27]　人家如你所见，在看店啊。\n"
"\\N[27]　还是老样子」"
msgstr ""

#. ID 1, Page 4, Line 16, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「不过话说回来，过上这样的日子后。\n"
"\\N[27]　之前那段动乱简直就象梦一样呢。\n"
"\\N[27]　现在都已经变得这么和平了」"
msgstr ""

#. ID 1, Page 4, Line 20, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「虽然，也有很多东西\n"
"\\N[27]　再也无法变回原来的样子了……」"
msgstr ""

#. ID 1, Page 4, Line 23, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「……听说，因为觉得已经没什么好做，\n"
"\\N[27]  有不少探险家都陆续离开了这里呢」"
msgstr ""

#. ID 1, Page 4, Line 26, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「虽然这也是理所当然，\n"
"\\N[27]　但总觉的有些寂寞啊。\n"
"\\N[27]　就好象节庆结束了似的」"
msgstr ""

#. ID 1, Page 4, Line 35, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「……我说啊」"
msgstr ""

#. ID 1, Page 4, Line 38, Pos (7,10)
#. Contains choice at line 3 (2 options)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「\\N[\\V[2]]是不会、离开这里的吧？」"
msgstr ""

#. ID 1, Page 4, Line 43, Pos (7,10)
#. Choice (2 options, embedded in a message)
msgid ""
"一直留在镇里\n"
"计划要离开"
msgstr ""

#. ID 1, Page 4, Line 46, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「……是嘛。\n"
"\\N[27]　太好了！果然是人家想多了」"
msgstr ""

#. ID 1, Page 4, Line 49, Pos (7,10)
#. ID 1, Page 4, Line 67, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「人家啊，那个呢，不知为什么，\n"
"\\N[27]　总是有种预感，觉得\\N[\\V[2]]\n"
"\\N[27]　有一天会突然从人家眼前消失掉」"
msgstr ""

#. ID 1, Page 4, Line 53, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「这种预感人家从小就有了，\n"
"\\N[27]　诶嘿嘿。这样怪怪的吧」"
msgstr ""

#. ID 1, Page 4, Line 56, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]　接下来嘛，\n"
"\\N[27]　人家就陪你去散个步吧」"
msgstr ""

#. ID 1, Page 4, Line 59, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「诶，看店？\n"
"\\N[27]　哎呀，那种事情无所谓啦」"
msgstr ""

#. ID 1, Page 4, Line 65, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「……果然啊」"
msgstr ""

#. ID 1, Page 4, Line 71, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「从还小的时候，就一直这么觉得……」"
msgstr ""

#. ID 1, Page 4, Line 73, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「……好，决定了」"
msgstr ""

#. ID 1, Page 4, Line 76, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「\\N[\\V[2]]要是想离开这个\n"
"\\N[27]　城镇的话，人家也一起走」"
msgstr ""

#. ID 1, Page 4, Line 79, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「因为…………」"
msgstr ""

#. ID 1, Page 4, Line 81, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「因为，人家啊……\n"
"\\N[27]　人家最喜欢、\\N[\\V[2]]了！」"
msgstr ""

#. ID 1, Page 4, Line 86, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「\\N[\\V[2]]要是想离开城镇的话，\n"
"\\N[27]　人家也一起跟着去吧」"
msgstr ""

#. ID 1, Page 4, Line 89, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「毕竟，如果没人在旁边关照着的话，\n"
"\\N[27]　以\\N[\\V[2]]你的性子，\n"
"\\N[27]　肯定会自己蛮干的嘛」"
msgstr ""

#. ID 1, Page 4, Line 96, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「就算你说不愿意也没用，\n"
"\\N[27]　人家就是要跟来！」"
msgstr ""

#. ID 1, Page 4, Line 104, Pos (7,10)
#. ID 8, Page 7, Line 81, Pos (12,10)
#. ID 28, Page 1, Line 213, Pos (9,11)
#. ID 28, Page 1, Line 274, Pos (9,11)
msgid "\\N[27]\\N[\\V[81]]成为了同伴！"
msgstr ""

#. ID 1, Page 4, Line 115, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「我说啊，\\N[6]。别总是\n"
"\\N[27]　做些男孩子气的事情了，\n"
"\\N[27]　差不多也考虑一下新娘修行的事吧」"
msgstr ""

#. ID 1, Page 4, Line 122, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「新娘～？\n"
"\\N[27]　唔嗯`……\n"
"\\N[27]　人家啊……你看，有这东西嘛」"
msgstr ""

#. ID 1, Page 4, Line 127, Pos (7,10)
msgid "\\N[27]\\N[6]用手指着你。"
msgstr ""

#. ID 1, Page 4, Line 130, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「是哦。\n"
"\\N[27]　有这东西就没问题了」"
msgstr ""

#. ID 1, Page 4, Line 134, Pos (7,10)
msgid "\\N[27]你被母女二人联合起来用东西称呼了。"
msgstr ""

#. ID 1, Page 4, Line 137, Pos (7,10)
msgid ""
"\\N[27]\\N[6]\n"
"\\N[27]「呜`……」"
msgstr ""

#. ID 1, Page 4, Line 143, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「最近城镇总算是平静下来了」"
msgstr ""

#. ID 1, Page 4, Line 146, Pos (7,10)
#. ID 1, Page 4, Line 151, Pos (7,10)
msgid ""
"\\N[27]杂货店主\n"
"\\N[27]「听说这都是\\N[\\V[2]]你们的功劳呢，\n"
"\\N[27]　是真的吗？」"
msgstr ""

