#. ID 1, Page 1, Line 5, Pos (0,0)
msgid ""
"\\N[27]※将光标移动到角色脸部再按下决定键，\n"
"\\N[27]　就可以和那名角色对话。"
msgstr ""

#. ID 2, Page 1, Line 21, Pos (0,1)
msgid "\\N[27]在那一夜DD"
msgstr ""

#. ID 2, Page 1, Line 23, Pos (0,1)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「\\N[\\V[2]]大人……\n"
"\\N[27]　\\N[\\V[2]]大人，请快起来」"
msgstr ""

#. ID 2, Page 1, Line 26, Pos (0,1)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「很抱歉半夜把您吵醒。\n"
"\\N[27]　但现在镇上发生了异变」"
msgstr ""

#. ID 2, Page 1, Line 34, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「冷静一点，慢慢讲。\n"
"\\N[27]　发生什么事了？」"
msgstr ""

#. ID 2, Page 1, Line 40, Pos (0,1)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「就是说那个，是怪物。\n"
"\\N[27]　身材矮小，像丑陋的小孩一样的怪物。\n"
"\\N[27]　它们拿着武器在到处袭击」"
msgstr ""

#. ID 2, Page 1, Line 44, Pos (0,1)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「怪物的力量与身材不成正比，我们已经有两个\n"
"\\N[27]　同伴受伤了。那些家伙简直就像……童话故事\n"
"\\N[27]　里描述的小鬼一样……」"
msgstr ""

#. ID 2, Page 1, Line 49, Pos (0,1)
msgid ""
"\\N[27]泽派克 \n"
"\\N[27]「怪物也有着相当众多的数量。\n"
"\\N[27]　看来它们是分散开来四处袭击了」"
msgstr ""

#. ID 2, Page 1, Line 53, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「即使是怪物，只要是肉长的，\n"
"\\N[27]　也就跟盗匪没什么两样」"
msgstr ""

#. ID 2, Page 1, Line 56, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「给我向边境警备队传令！泽派克，\n"
"\\N[27]　你率领士兵出击。一边招集散落\n"
"\\N[27]　在镇上的自警团，一边扫荡敌人」"
msgstr ""

#. ID 2, Page 1, Line 61, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[\\V[2]]。你去镇上巡逻，\n"
"\\N[27]　救援那些被袭击的镇民！」"
msgstr ""

#. ID 2, Page 1, Line 66, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[\\V[2]]。你要远离危险地区，\n"
"\\N[27]   找个安全的地方躲起来」"
msgstr ""

#. ID 2, Page 2, Line 6, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「哦哦，你来得正好。\n"
"\\N[27]　\\N[\\V[2]]，有位大人正想要见你一面」"
msgstr ""

#. ID 2, Page 2, Line 12, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「初次见面，\\N[\\V[2]]先生。\n"
"\\N[27]　很荣幸能够与你相识」"
msgstr ""

#. ID 2, Page 2, Line 16, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「咳咳。唔`，\\N[11]殿下\n"
"\\N[27]　出身自继承了大公血脉的名门。\n"
"\\N[27]　为救济公国，特地前来参加遗迹探索」"
msgstr ""

#. ID 2, Page 2, Line 21, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「就是这样，我的领地现在也\n"
"\\N[27]　变得一团混乱了。既然人民有难，\n"
"\\N[27]　我身为骑士，理当挺身而出」"
msgstr ""

#. ID 2, Page 2, Line 25, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「听说这里有奇怪的怪物出没，\n"
"\\N[27]　不过那种东西我只要喝啊！哈啊！\n"
"\\N[27]　几下就能解决了！」"
msgstr ""

#. ID 2, Page 2, Line 29, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「\\N[\\V[2]]也会参加探索的吧？\n"
"\\N[27]　让我们并肩作战吧！」"
msgstr ""

#. ID 2, Page 2, Line 33, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「咳咳。那个，能够与\\N[11]殿下\n"
"\\N[27]　建立友谊，对我等家族来说\n"
"\\N[27]　实在是莫大的荣誉」"
msgstr ""

#. ID 2, Page 2, Line 37, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「在战场上建立的友谊，\n"
"\\N[27]　胜过在交际所培养十年的关系。\n"
"\\N[27]　能帮到您，对我们也是一大幸事」"
msgstr ""

#. ID 2, Page 2, Line 42, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「我也会加油的！」"
msgstr ""

#. ID 2, Page 2, Line 44, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「那我先走一步，去酒馆里\n"
"\\N[27]　找找看其他同伴。\n"
"\\N[27]　我们再会吧」"
msgstr ""

#. ID 2, Page 2, Line 50, Pos (0,1)
#. ID 2, Page 3, Line 144, Pos (0,1)
#. ID 2, Page 4, Line 290, Pos (0,1)
#. ID 4, Page 8, Line 44, Pos (15,7)
#. ID 4, Page 8, Line 83, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「…………」"
msgstr ""

#. ID 2, Page 2, Line 52, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「呃、那个嘛。搞不好要\n"
"\\N[27]　辛苦\\N[\\V[2]]你一下了。\n"
"\\N[27]　拜托你千万照顾好他」"
msgstr ""

#. ID 2, Page 2, Line 59, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「初次见面，\\N[\\V[2]]小姐。\n"
"\\N[27]　哎呀，你可真是美丽啊。\n"
"\\N[27]　能认识你是我的荣幸！」"
msgstr ""

#. ID 2, Page 2, Line 64, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「哈哈哈，\\N[11]殿下\n"
"\\N[27]　出身自继承了大公血脉的名门。\n"
"\\N[27]　为救济公国，特地前来参加遗迹探索」"
msgstr ""

#. ID 2, Page 2, Line 68, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「能与\\N[11]殿下\n"
"\\N[27]　建立友谊，对我等家族来说\n"
"\\N[27]　实在是莫大的荣誉啊，哈哈哈」"
msgstr ""

#. ID 2, Page 2, Line 73, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「对我来说也是一件幸事。\n"
"\\N[27]　因为能和这样一位美丽的小姐结下良缘」"
msgstr ""

#. ID 2, Page 2, Line 77, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「哈哈哈。哎呀，这事虽然\n"
"\\N[27]　还没有决定，不过如果真能实现\n"
"\\N[27]　那对双方的家族来说都是好事啊」"
msgstr ""

#. ID 2, Page 2, Line 81, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「不过呃、那个啊、我家这个女儿\n"
"\\N[27]　实在是很调皮，只怕您会看不上\n"
"\\N[27]　她啊，哈哈哈」"
msgstr ""

#. ID 2, Page 2, Line 86, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「没问题，我不在乎那些的！\n"
"\\N[27]　我喜欢强大的女性。让我们一起去探索，\n"
"\\N[27]　把怪物们打个落花流水吧！」"
msgstr ""

#. ID 2, Page 2, Line 90, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「为拯救人民于水生火热之中，\n"
"\\N[27]　让我们共同奋进吧！能拯救这个\n"
"\\N[27]　公国的正是我们的爱，呵呵，说笑的」"
msgstr ""

#. ID 2, Page 2, Line 95, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「哈哈哈。不要太得寸进尺了，臭小子」"
msgstr ""

#. ID 2, Page 2, Line 98, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「诶？」"
msgstr ""

#. ID 2, Page 2, Line 101, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「不不、我什么都没说。哈哈哈」"
msgstr ""

#. ID 2, Page 2, Line 104, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「是这样吗？\n"
"\\N[27]　那我先走一步，到酒馆里\n"
"\\N[27]　去找找其他的探险伙伴吧」"
msgstr ""

#. ID 2, Page 2, Line 108, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「我们再会吧！」"
msgstr ""

#. ID 2, Page 2, Line 112, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……………………」"
msgstr ""

#. ID 2, Page 2, Line 114, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「………………抱歉。\n"
"\\N[27]　你不用勉强自己的…………」"
msgstr ""

#. ID 2, Page 3, Line 7, Pos (0,1)
msgid "\\N[27]当你回到馆中时，察觉到这里的气氛与平时不同。"
msgstr ""

#. ID 2, Page 3, Line 9, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……你回来了啊」"
msgstr ""

#. ID 2, Page 3, Line 12, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[130]殿下。\n"
"\\N[27]　那就是在下的犬子，\n"
"\\N[27]　名叫\\N[\\V[2]]」"
msgstr ""

#. ID 2, Page 3, Line 18, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[130]殿下。\n"
"\\N[27]　那就是在下的小女，\n"
"\\N[27]　名叫\\N[\\V[2]]」"
msgstr ""

#. ID 2, Page 3, Line 25, Pos (0,1)
#. ID 2, Page 3, Line 67, Pos (0,1)
msgid ""
"\\N[27]在台阶上站着一个陌生的身影。\n"
"\\N[27]那是一位蓄着优美的胡鬓，身着华丽铠甲的贵公子，\n"
"\\N[27]他一边怠傲地扶着楼梯扶手，\n"
"\\N[27]一边漫不经心地向你投来俯视的目光。"
msgstr ""

#. ID 2, Page 3, Line 31, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哦呀……\n"
"\\N[27]　我们又见面了」"
msgstr ""

#. ID 2, Page 3, Line 36, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哦？你就是……」"
msgstr ""

#. ID 2, Page 3, Line 39, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「余乃\\N[130]。\n"
"\\N[27]　涅斯公国大公之子」"
msgstr ""

#. ID 2, Page 3, Line 45, Pos (0,1)
#. ID 2, Page 3, Line 80, Pos (0,1)
msgid "\\N[27]你注意到贵公子的脸上，刻着一道十字型的伤痕。"
msgstr ""

#. ID 2, Page 3, Line 50, Pos (0,1)
msgid "\\N[27]你要进入馆邸之时，却被管家阻止了。"
msgstr ""

#. ID 2, Page 3, Line 52, Pos (0,1)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「今天有贵客前来，\n"
"\\N[27]　领主大人无法再接待你们了。\n"
"\\N[27]　不好意思，今天就请先回去吧」"
msgstr ""

#. ID 2, Page 3, Line 57, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「无妨。\n"
"\\N[27]　余正想与勇于探索遗迹，\n"
"\\N[27]　和怪物奋战的探索者们直接交谈」"
msgstr ""

#. ID 2, Page 3, Line 62, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「况且，也不是素未相识之人了」"
msgstr ""

#. ID 2, Page 3, Line 73, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「余乃\\N[130]。\n"
"\\N[27]　涅斯公国大公之子」\n"
"\\N[27]　此番前来是为了调查遗迹」"
msgstr ""

#. ID 2, Page 3, Line 88, Pos (0,1)
#. ID 22, Page 1, Line 4, Pos (11,5)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「好久不见了，\\N[130]。\n"
"\\N[27]　你也到这里来了啊」"
msgstr ""

#. ID 2, Page 3, Line 92, Pos (0,1)
#. ID 22, Page 1, Line 8, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哦，是\\N[11]啊」"
msgstr ""

#. ID 2, Page 3, Line 94, Pos (0,1)
#. ID 22, Page 1, Line 10, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「你还是一如既往地精力充沛，如此甚好，\n"
"\\N[27]　可有好好履行自己的职责？」"
msgstr ""

#. ID 2, Page 3, Line 98, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「嗯嗯。我为了公国\n"
"\\N[27]　每天都在努力奋战」"
msgstr ""

#. ID 2, Page 3, Line 101, Pos (0,1)
#. ID 22, Page 1, Line 17, Pos (11,5)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「啊，忘了说，\\N[130]公子和我是堂兄弟」"
msgstr ""

#. ID 2, Page 3, Line 103, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「他这个人有些怪怪的，经常会被别人\n"
"\\N[27]　误会，不过其实是个心怀大志的人。\n"
"\\N[27]　我一直都很崇拜他」"
msgstr ""

#. ID 2, Page 3, Line 108, Pos (0,1)
#. ID 22, Page 1, Line 24, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哈哈哈，别这么抬举余了」"
msgstr ""

#. ID 2, Page 3, Line 112, Pos (0,1)
#. ID 22, Page 1, Line 28, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「说起来，\\N[\\V[2]]殿下」"
msgstr ""

#. ID 2, Page 3, Line 114, Pos (0,1)
#. ID 22, Page 1, Line 30, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「阁下与\\N[11]之间\n"
"\\N[27]　确实有谈过提亲之事吧。\n"
"\\N[27]　如何，阁下有那个意思吗？」"
msgstr ""

#. ID 2, Page 3, Line 119, Pos (0,1)
#. ID 22, Page 1, Line 35, Pos (11,5)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「啊、\\N[130]、\n"
"\\N[27]　怎么突然谈起这个……」"
msgstr ""

#. ID 2, Page 3, Line 123, Pos (0,1)
#. ID 22, Page 1, Line 39, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哈哈哈。最初提起你与\\N[11]\n"
"\\N[27]　之间亲事的可是余本人啊。\n"
"\\N[27]　要是没能谈妥，余就很没面子了」"
msgstr ""

#. ID 2, Page 3, Line 128, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「这件亲事确实是承您之美……\n"
"\\N[27]　不过我觉得，还是交给两个\n"
"\\N[27]　当事人慢慢自己决定为好」"
msgstr ""

#. ID 2, Page 3, Line 133, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哈哈、这种事就应该交给年轻人，\n"
"\\N[27]　说的也是啊」"
msgstr ""

#. ID 2, Page 3, Line 136, Pos (0,1)
#. ID 22, Page 1, Line 43, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「……终有一天，大公和各有力诸侯的\n"
"\\N[27]　力量会合而为一，余相信这种时代会到来」"
msgstr ""

#. ID 2, Page 3, Line 139, Pos (0,1)
#. ID 22, Page 1, Line 46, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「所以婚姻大事要慎重考虑。\n"
"\\N[27]　这世上没有什么比拆散相依相恋\n"
"\\N[27]　的男女更难的事情了。哈哈哈！」"
msgstr ""

#. ID 2, Page 3, Line 147, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「不过，这一切也得等到这场\n"
"\\N[27]　骚乱结束之后DD」"
msgstr ""

#. ID 2, Page 3, Line 161, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「余此番前来是为了\n"
"\\N[27]　亲自监督遗迹的探索情况」"
msgstr ""

#. ID 2, Page 3, Line 164, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「……自遗迹引发灾祸起，\n"
"\\N[27]　各地的诸侯纷纷动摇，\n"
"\\N[27]　连邻近国家也蠢蠢欲动」"
msgstr ""

#. ID 2, Page 3, Line 168, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「余等涅斯公国是由各诸侯领结成的联邦\n"
"\\N[27]　国家。若在遭受外敌侵袭之际无法团结\n"
"\\N[27]　一致，整个国家将难逃覆灭之危」"
msgstr ""

#. ID 2, Page 3, Line 172, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「那种结局对我们来说才是真正的灾祸。\n"
"\\N[27]　为了避免如此，我们必须要推进探索，\n"
"\\N[27]　找出终结这场异变的方法」"
msgstr ""

#. ID 2, Page 3, Line 177, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「余已命令麾下的＜火车骑士团＞\n"
"\\N[27]　在城镇附近退治怪物。你们也要\n"
"\\N[27]　尽力而为，不要辜负公国的期望」"
msgstr ""

#. ID 2, Page 4, Line 7, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[\\V[2]]吗。\n"
"\\N[27]　我一直在等你回来」"
msgstr ""

#. ID 2, Page 4, Line 12, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[\\V[2]]。\n"
"\\N[27]　等一等」"
msgstr ""

#. ID 2, Page 4, Line 17, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「今夜有贵客要光临。\n"
"\\N[27]　从尤鲁弗莱尔岛的大河神殿远道而来\n"
"\\N[27]　的巴鲁斯穆斯大神官」"
msgstr ""

#. ID 2, Page 4, Line 21, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「他被人们通称为＜战将巴鲁斯穆斯＞。\n"
"\\N[27]　是一位率领圣骑士与异教徒英勇作战，\n"
"\\N[27]　建立了无数战功的大人物」"
msgstr ""

#. ID 2, Page 4, Line 26, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「……这位大神官殿下主张，\n"
"\\N[27]　他们是为了确认异变的状况而来」"
msgstr ""

#. ID 2, Page 4, Line 29, Pos (0,1)
#. ID 22, Page 6, Line 71, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「但是，若仅仅是为了视察，\n"
"\\N[27]　神殿绝对不会把拥有＜战将＞之名\n"
"\\N[27]　的激进武斗派派遣至此」"
msgstr ""

#. ID 2, Page 4, Line 35, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「今天晚上我们会与他共进晚餐。\n"
"\\N[27]　\\N[\\V[2]]，你也出席吧」"
msgstr ""

#. ID 2, Page 4, Line 42, Pos (0,1)
#. Choice (2 options)
msgid ""
"还有事\n"
"参加"
msgstr ""

#. ID 2, Page 4, Line 46, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「这样啊。\n"
"\\N[27]　那你就尽快做完，\n"
"\\N[27]　早点回来」"
msgstr ""

#. ID 2, Page 4, Line 64, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「那就做好准备吧」"
msgstr ""

#. ID 2, Page 4, Line 72, Pos (0,1)
msgid ""
"\\N[27]夜幕降临的时候，晚宴已经准备齐全\n"
"\\N[27]你也被叫到了席上。"
msgstr ""

#. ID 2, Page 4, Line 81, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯\n"
"\\N[27]「哦呀，阁下是……\n"
"\\N[27]　刚才我们见过面的吧」"
msgstr ""

#. ID 2, Page 4, Line 87, Pos (0,1)
msgid ""
"\\N[27]晚宴的主角，曾与你在白天有过短暂的交谈。\n"
"\n"
"\\N[27]你们互相打了招呼。"
msgstr ""

#. ID 2, Page 4, Line 94, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯\n"
"\\N[27]「今次本是冒昧访问，\n"
"\\N[27]　居然还被如此盛情接待，\n"
"\\N[27]　实在不胜荣幸」"
msgstr ""

#. ID 2, Page 4, Line 101, Pos (0,1)
msgid ""
"\\N[27]来客是虽已秃顶，却有着健硕体格的老神官。\n"
"\\N[27]他长相凶暴，额头上留有刀疤，耳垂也已残缺不全。\n"
"\\N[27]明明是如此凶恶的外貌，却因他安详的眼神和语调，\n"
"\\N[27]反给人一种诚恳的感觉。"
msgstr ""

#. ID 2, Page 4, Line 106, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哈哈哈，无须这么客气」"
msgstr ""

#. ID 2, Page 4, Line 109, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「如公子所言。能接待久负盛名的\n"
"\\N[27]　大神官巴鲁斯穆斯殿下，\n"
"\\N[27]　我等才是不胜荣幸」"
msgstr ""

#. ID 2, Page 4, Line 114, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「长途跋涉已经很累了吧。\n"
"\\N[27]　请把鄙馆当作自己家一样放松休息」"
msgstr ""

#. ID 2, Page 4, Line 120, Pos (0,1)
msgid ""
"\\N[27]卡穆罗、\\N[130]、巴鲁斯穆斯一行和你，\n"
"\\N[27]一边进餐一边进行着交谈。\n"
"\\N[27]不知从什么时候开始，意味深长的沉默在席间飘荡，\n"
"\\N[27]餐桌周围充满了紧张感。"
msgstr ""

#. ID 2, Page 4, Line 125, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯\n"
"\\N[27]「DD继南方的荒野地区之后，\n"
"\\N[27]　艾鲁帕迪亚与异教徒之间的战斗，\n"
"\\N[27]　也暂时告一段落了」"
msgstr ""

#. ID 2, Page 4, Line 129, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「贫僧也曾经几次作为援军奔赴其中，\n"
"\\N[27]　那地方的战况相当严峻，到停战为止，\n"
"\\N[27]　河南的都市都已失去了几座」"
msgstr ""

#. ID 2, Page 4, Line 133, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「僧兵的责任本该是守护信奉\n"
"\\N[27]　大河女神的国民国土。\n"
"\\N[27]　这次却没能尽职，真是丢尽了老脸」"
msgstr ""

#. ID 2, Page 4, Line 138, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「没这回事，要是没有巴鲁斯穆斯殿下\n"
"\\N[27]　的努力，被害情况肯定会比现在严重。\n"
"\\N[27]　您的活跃已经远播到我这块偏僻小地了」"
msgstr ""

#. ID 2, Page 4, Line 143, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「能蒙您如此称道，贫僧真感到欣慰」"
msgstr ""

#. ID 2, Page 4, Line 145, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「说起来，这城镇真是意外地平稳啊。\n"
"\\N[27]　贫僧本以为怪物的袭击和农田的遭灾，\n"
"\\N[27]　会让镇里的治安极度恶化的」"
msgstr ""

#. ID 2, Page 4, Line 150, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「还远不能算是平稳呢。\n"
"\\N[27]　能保持现在这样也是多亏\\N[130]殿下\n"
"\\N[27]　的骑士团从首都赶来支援」"
msgstr ""

#. ID 2, Page 4, Line 154, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「要是遗迹的调查能有所进展，\n"
"\\N[27]　找出灾祸的原因将其排除就好了。\n"
"\\N[27]　可惜在这方面也还没什么成果」"
msgstr ""

#. ID 2, Page 4, Line 159, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「原来如此。\n"
"\n"
"\\N[27]　……卡穆罗卿，贫僧有一个提案」"
msgstr ""

#. ID 2, Page 4, Line 163, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「那遗迹的调查和管理，\n"
"\\N[27]　能否交由我等大河神殿负责？」"
msgstr ""

#. ID 2, Page 4, Line 167, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「您的意思是……」"
msgstr ""

#. ID 2, Page 4, Line 170, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「交由来历不明的探索者负责此事，\n"
"\\N[27]　恐怕只会让有用的线索白白流失」"
msgstr ""

#. ID 2, Page 4, Line 173, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「那群人仅以盗掘遗迹的财宝为目的，\n"
"\\N[27]　交由他们负责，调查无法进展也是\n"
"\\N[27]　理所当然之事」"
msgstr ""

#. ID 2, Page 4, Line 177, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「但如果交由神殿管理，我等就可以派遣\n"
"\\N[27]　学识丰富的学僧和英勇善战的圣战士。\n"
"\\N[27]　此事还是交由我等负责吧」"
msgstr ""

#. ID 2, Page 4, Line 184, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哈哈哈……\n"
"\\N[27]　原来你们打的是这个主意」"
msgstr ""

#. ID 2, Page 4, Line 190, Pos (0,1)
msgid "\\N[27]原本沉默不语的\\N[130]，却唐突地笑出了声。"
msgstr ""

#. ID 2, Page 4, Line 192, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[130]殿下……？」"
msgstr ""

#. ID 2, Page 4, Line 195, Pos (0,1)
#. ID 2, Page 4, Line 236, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「…………」"
msgstr ""

#. ID 2, Page 4, Line 198, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「简而言之，就是神殿不满\n"
"\\N[27]　余等独占遗迹和其中的古代知识吧？」"
msgstr ""

#. ID 2, Page 4, Line 201, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「若是想来抢夺遗迹，\n"
"\\N[27]　你就尽管直说好了」"
msgstr ""

#. ID 2, Page 4, Line 205, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「…………并非如此」"
msgstr ""

#. ID 2, Page 4, Line 207, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「您所说的古代智慧，\n"
"\\N[27]　实乃凶险万分之物」"
msgstr ""

#. ID 2, Page 4, Line 210, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「昔日阿尔凯亚的帝国皇帝，\n"
"\\N[27]　虽支配了整个世界，\n"
"\\N[27]　却也因为傲慢，被神明所消灭」"
msgstr ""

#. ID 2, Page 4, Line 214, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「他们生而为人，却又试图超越人之界限，\n"
"\\N[27]　终于连众神的领域都为他们所侵犯」"
msgstr ""

#. ID 2, Page 4, Line 217, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「若是那个时代的思想被现代人所知晓，\n"
"\\N[27]　未来终有一天会引发同样的灾祸，\n"
"\\N[27]　介时神罚将会再次降临于世」"
msgstr ""

#. ID 2, Page 4, Line 221, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「您毫不考虑其中影响，\n"
"\\N[27]　就将那个时代的文献散布于世。\n"
"\\N[27]　我等无法对此坐视不理」"
msgstr ""

#. ID 2, Page 4, Line 226, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「是嘛」"
msgstr ""

#. ID 2, Page 4, Line 229, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「您理解了吗」"
msgstr ""

#. ID 2, Page 4, Line 232, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「无聊。\n"
"\\N[27]　简直腐朽透顶」"
msgstr ""

#. ID 2, Page 4, Line 239, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「阁下等人只不过是，\n"
"\\N[27]　担心神殿的权威会因此受到威胁而已」"
msgstr ""

#. ID 2, Page 4, Line 242, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「生为人而超越人之境，\n"
"\\N[27]　直至遥远的神之领域。\n"
"\\N[27]　这不是很棒的想法吗」"
msgstr ""

#. ID 2, Page 4, Line 246, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「过去的人们，曾经造就了一个伟大\n"
"\\N[27]　的种族。余只是将此事广为传扬，\n"
"\\N[27]　究竟有何不可？」"
msgstr ""

#. ID 2, Page 4, Line 250, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「向往高处是人之本性。\n"
"\\N[27]　即便想要强行将其压制，\n"
"\\N[27]　也终会抑制不住」"
msgstr ""

#. ID 2, Page 4, Line 255, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「……阁下，对自己所做之事\n"
"\\N[27]　的真意，早已有所自觉吗」"
msgstr ""

#. ID 2, Page 4, Line 259, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「自是如此」"
msgstr ""

#. ID 2, Page 4, Line 263, Pos (0,1)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「既然如此，与您之间的对话已经毫\n"
"\\N[27]　无意义。尽管有失礼节，但还请允\n"
"\\N[27]　许贫僧就此离席」"
msgstr ""

#. ID 2, Page 4, Line 271, Pos (0,1)
msgid ""
"\\N[27]老神官站起身来，与自己少数的随从一起，\n"
"\\N[27]在夜幕之中毅然离去。"
msgstr ""

#. ID 2, Page 4, Line 274, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「这只秃鹫。\n"
"\\N[27]　装作大义凌然的样子，\n"
"\\N[27]　实际上却是想抢夺遗迹」"
msgstr ""

#. ID 2, Page 4, Line 278, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「有关阿尔凯亚帝国的记录几乎都\n"
"\\N[27]　没能留存下来，如此来看，其中原因\n"
"\\N[27]　大有可能是大河神殿在有意掩盖」"
msgstr ""

#. ID 2, Page 4, Line 283, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[130]殿下……\n"
"\\N[27]　为什么要做出与神殿敌对的行为？」"
msgstr ""

#. ID 2, Page 4, Line 287, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「与神殿敌对，又有何不可了？」"
msgstr ""

#. ID 2, Page 4, Line 293, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「别那么为难。那群和尚\n"
"\\N[27]　只不过是沉溺在自己的权威性中而已，\n"
"\\N[27]　他们无法容忍世事超脱出自己的掌控」"
msgstr ""

#. ID 2, Page 4, Line 297, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「如果真的接受他们的要求，在遗迹之后，\n"
"\\N[27]　下一个被夺取的就会是这个城镇了」"
msgstr ""

#. ID 2, Page 4, Line 300, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「你要知道，保护镇上的居民\n"
"\\N[27]　可并不在他们的考虑当中」"
msgstr ""

#. ID 2, Page 4, Line 305, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]（DD但是啊，\\N[130]殿下。\n"
"\\N[27]　您又有把镇民们放在心上吗DD？"
msgstr ""

#. ID 2, Page 4, Line 311, Pos (0,1)
msgid ""
"\\N[27]卡穆罗露出一副欲言又止的表情，\n"
"\\N[27]然后陷入了长久的沉默DD"
msgstr ""

#. ID 2, Page 4, Line 321, Pos (0,1)
msgid "\\>\\V[31]日目　王国历\\V[32]年　\\V[33]月　\\V[34]日\\<"
msgstr ""

#. ID 2, Page 4, Line 326, Pos (0,1)
msgid ""
"\\N[27]泽派克 \n"
"\\N[27]「不好意思，本馆有贵客要来访。\n"
"\\N[27]　据说是从尤鲁弗莱尔岛的大河神殿\n"
"\\N[27]　远道而来的巴鲁斯穆斯大神官」"
msgstr ""

#. ID 2, Page 4, Line 331, Pos (0,1)
msgid ""
"\\N[27]泽派克 \n"
"\\N[27]「因此，馆内暂时不允许随意出入」"
msgstr ""

#. ID 2, Page 4, Line 341, Pos (0,1)
msgid "\\N[27]你被赶了出来……"
msgstr ""

#. ID 2, Page 5, Line 24, Pos (0,1)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……\\N[\\V[2]]大人，\n"
"\\N[27]　请您快醒醒。\n"
"\\N[27]　馆主大人说，要您尽快去大厅一趟……」"
msgstr ""

#. ID 2, Page 5, Line 32, Pos (0,1)
msgid ""
"\\N[27]这天清晨，你比平时醒得更早，\n"
"\\N[27]令人倒胃的紧张感在空气中弥漫着，\n"
"\\N[27]周围已经是慌乱一片。"
msgstr ""

#. ID 2, Page 5, Line 36, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「啊，\\N[\\V[2]]……\n"
"\\N[27]　大事不好了。\n"
"\\N[27]　战争好象要爆发了」"
msgstr ""

#. ID 2, Page 5, Line 41, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「黎明之前，敌人侵入了我们西方的国境。\n"
"\\N[27]　敌人是DD我们的邻国，西席瓦尔王国」"
msgstr ""

#. ID 2, Page 5, Line 44, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「停战已持续了三十年，他们为何要\n"
"\\N[27]　突然撕毁协议，对我们发动突袭……」"
msgstr ""

#. ID 2, Page 5, Line 47, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「现在国境警备团正在迎战，\n"
"\\N[27]　接到通知之后，\\N[130]殿下\n"
"\\N[27]　也立刻派骑士团去支援了」"
msgstr ""

#. ID 2, Page 5, Line 52, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「我们就留守在这里吧。\n"
"\\N[27]　\\N[\\V[2]]，你去参加城镇的防护」"
msgstr ""

#. ID 2, Page 5, Line 66, Pos (0,1)
msgid ""
"\\N[27]正在此时，神殿的钟楼里\n"
"\\N[27]突然传出了钟鸣之声。\n"
"\\N[27]那是预示危险的警报声。"
msgstr ""

#. ID 2, Page 5, Line 69, Pos (0,1)
msgid "\\N[27]镇里的巡逻兵猛地跑进了馆中。"
msgstr ""

#. ID 2, Page 5, Line 70, Pos (0,1)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「敌DD敌袭！　有敌袭！\n"
"\\N[27]　敌人的船队在西部的港口登陆了！」"
msgstr ""

#. ID 2, Page 5, Line 75, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「怎么可能！\n"
"\\N[27]　可恶的席瓦尔人……」"
msgstr ""

#. ID 2, Page 5, Line 81, Pos (0,1)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「不、那是……」"
msgstr ""

#. ID 2, Page 5, Line 83, Pos (0,1)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「敌人的旗帜是DD神殿军！\n"
"\\N[27]　是大河神殿的圣战士团！\n"
"\\N[27]　他们和席瓦尔军一起发动了袭击！」"
msgstr ""

#. ID 2, Page 5, Line 92, Pos (0,1)
msgid ""
"\\N[27]你跑到外面观望，发现镇西和镇南燃起了烟火。\n"
"\\N[27]身穿陌生铠甲的敌兵在城镇的大道上驰骋跋扈。"
msgstr ""

#. ID 2, Page 5, Line 95, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「这是……」"
msgstr ""

#. ID 2, Page 5, Line 97, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……没想到神殿居然做到如此地步。\n"
"\\N[27]　凭镇上巡逻队的数量根本就无法\n"
"\\N[27]　与之抗衡……」"
msgstr ""

#. ID 2, Page 5, Line 102, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「即使如此，我等骑士也必须挺身而战」"
msgstr ""

#. ID 2, Page 5, Line 107, Pos (0,1)
msgid ""
"\\N[27]DD终于，镇上的守卫队被敌人击破。\n"
"\\N[27]敌兵杀到了城馆的入口！"
msgstr ""

#. ID 2, Page 5, Line 115, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「呜……！」"
msgstr ""

#. ID 2, Page 5, Line 120, Pos (0,1)
msgid ""
"\\N[27]即使打倒眼前的敌人，新的敌人也会\n"
"\\N[27]连接不断地出现，你们就要被绝望的数量差所压倒。\n"
"\\N[27]城馆的陷落恐怕也只是时间的问题了。"
msgstr ""

#. ID 2, Page 5, Line 124, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「到此为止了吗……」"
msgstr ""

#. ID 2, Page 5, Line 126, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……\\N[\\V[2]]。\n"
"\\N[27]　你用馆中的地下通道\n"
"\\N[27]　逃到外面去吧」"
msgstr ""

#. ID 2, Page 5, Line 131, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「然后和\\N[11]殿下，\\N[13]他们一起\n"
"\\N[27]　躲藏起来，等待援军」"
msgstr ""

#. ID 2, Page 5, Line 137, Pos (0,1)
msgid ""
"\\N[27]听到父亲要你独自逃离，你动摇不已，\n"
"\\N[27]卡穆罗却突然提高声音吼了出来。"
msgstr ""

#. ID 2, Page 5, Line 140, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「快走啊，你这蠢货！\n"
"\\N[27]　你想让\\N[11]殿下和\\N[13]\n"
"\\N[27]　也留在这里白白送死吗！」"
msgstr ""

#. ID 2, Page 5, Line 145, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「暂且别过吧，我的好孩子。\n"
"\n"
"\\N[27]　快走！」"
msgstr ""

#. ID 2, Page 5, Line 152, Pos (0,1)
msgid ""
"\\N[27]像是被父亲的声音吓到了一样，\n"
"\\N[27]你们慌张地逃离了馆邸……"
msgstr ""

#. ID 2, Page 5, Line 156, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……\\N[\\V[2]]，你听好」"
msgstr ""

#. ID 2, Page 5, Line 158, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「你要逃出这里，找地方躲起来，\n"
"\\N[27]　然后等待救援的到来」"
msgstr ""

#. ID 2, Page 5, Line 161, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「这就是我给你的任务。\n"
"\\N[27]　明白了吗……？」"
msgstr ""

#. ID 2, Page 5, Line 164, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[11]殿下和\\N[13]\n"
"\\N[27]　也与\\N[\\V[2]]一起藏身吧」"
msgstr ""

#. ID 2, Page 5, Line 168, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「诶诶！？\n"
"\\N[27]　但是，明明敌人就在眼前\n"
"\\N[27]　不战而逃的话，骑士的荣誉就……」"
msgstr ""

#. ID 2, Page 5, Line 173, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[11]殿下，\n"
"\\N[27]　此时暂且逃脱是必要之事」"
msgstr ""

#. ID 2, Page 5, Line 176, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[\\V[2]]就拜托你照顾了」"
msgstr ""

#. ID 2, Page 5, Line 179, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「…………\n"
"\\N[27]　好的……」"
msgstr ""

#. ID 2, Page 5, Line 184, Pos (0,1)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「就说到这吧，要暂时分别了，好孩子。\n"
"\n"
"\\N[27]　快走！」"
msgstr ""

#. ID 2, Page 5, Line 191, Pos (0,1)
msgid "\\N[27]你从馆中逃出，为寻找藏身之地而全力向森林奔跑。"
msgstr ""

#. ID 2, Page 5, Line 193, Pos (0,1)
msgid "\\N[27]但是，你碰上了渴求着猎物的敌军！"
msgstr ""

#. ID 2, Page 5, Line 198, Pos (0,1)
msgid "\\N[27]你将追赶而来的敌兵全数击倒，逃进了森林里……"
msgstr ""

#. ID 2, Page 5, Line 201, Pos (0,1)
msgid "\\N[27]你从敌兵手中逃脱了……"
msgstr ""

#. ID 2, Page 5, Line 211, Pos (0,1)
msgid ""
"\\N[27]神殿军终于攻陷城馆，占领了城镇。\n"
"\\N[27]不久后，西席瓦尔军也前来汇合。\n"
"\\N[27]镇上的骚乱持续了好几天才逐渐平息。"
msgstr ""

#. ID 2, Page 5, Line 214, Pos (0,1)
msgid ""
"\\N[27]在镇里的状况稍稍缓和后，你到港口附近\n"
"\\N[27]的郊区藏起身来，等待着公国军的救援。"
msgstr ""

#. ID 2, Page 5, Line 229, Pos (0,1)
#. ID 3, Page 4, Line 50, Pos (16,13)
msgid "\\>第\\V[31]日　王国历\\V[32]年　\\V[33]月　\\V[34]日\\<"
msgstr ""

#. ID 3, Page 4, Line 4, Pos (16,13)
#. Contains choice at line 2 (2 options)
msgid "\\N[27]要进入梦乡，结束这段故事吗？"
msgstr ""

#. ID 3, Page 4, Line 5, Pos (16,13)
#. Choice (2 options, embedded in a message)
#. ID 12, Page 9, Line 79, Pos (6,9)
#. Choice (2 options)
#. ID 12, Page 9, Line 406, Pos (6,9)
#. Choice (2 options)
#. ID 23, Page 4, Line 81, Pos (14,8)
#. Choice (2 options)
#. ID 23, Page 4, Line 113, Pos (14,8)
#. Choice (2 options)
msgid ""
"是\n"
"否"
msgstr ""

#. ID 3, Page 4, Line 21, Pos (16,13)
#. Contains choice at line 2 (2 options)
msgid "\\N[27]今天已经要休息了吗？"
msgstr ""

#. ID 3, Page 4, Line 22, Pos (16,13)
#. Choice (2 options, embedded in a message)
#. ID 8, Page 6, Line 16, Pos (11,9)
#. Choice (2 options)
msgid ""
"\\N[27]是\n"
"\\N[27]否"
msgstr ""

#. ID 4, Page 1, Line 7, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「DD\\N[\\V[2]]。\n"
"\\N[27]　你不去牧草地帮忙巡视，\n"
"\\N[27]　一大清早到底跑到哪去了」"
msgstr ""

#. ID 4, Page 1, Line 11, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「不说我猜得到，反正又是跟镇上的朋友\n"
"\\N[27]　一起到什么地方胡闹去了吧」"
msgstr ""

#. ID 4, Page 1, Line 15, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「身为被托付了这片领土的骑士子嗣，\n"
"\\N[27]　你就不为自己的行为感到羞愧吗？」"
msgstr ""

#. ID 4, Page 1, Line 20, Pos (15,7)
#. ID 4, Page 1, Line 56, Pos (15,7)
msgid ""
"\\N[27]这个人是霍尔姆伯爵卡穆罗，\n"
"\\N[27]他是这个镇的领主。\n"
"\n"
"\\N[27]也是你的父亲。"
msgstr ""

#. ID 4, Page 1, Line 25, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……你一副有话要说的表情啊。\n"
"\\N[27]　其中有什么理由，我且听你说说吧」"
msgstr ""

#. ID 4, Page 1, Line 28, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「DD唔。在森林中，几天前的闪电\n"
"\\N[27]　偶然打开了一个洞穴的入口吗。\n"
"\\N[27]　你说里面还住着奇妙的生物？」"
msgstr ""

#. ID 4, Page 1, Line 32, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「唔……这样的话，在镇上出现受害者\n"
"\\N[27]　之前，我们这边先备好对策才比较妥当」"
msgstr ""

#. ID 4, Page 1, Line 35, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[\\V[2]]，这事就交给你了。\n"
"\\N[27]　你去调查洞穴，如果发现了\n"
"\\N[27]　危险因素，就要当即排除」"
msgstr ""

#. ID 4, Page 1, Line 39, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「这份职责，你可要好好完成啊」"
msgstr ""

#. ID 4, Page 1, Line 47, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「DD\\N[\\V[2]]！\n"
"\\N[27]　你跑到什么地方去了！」"
msgstr ""

#. ID 4, Page 1, Line 51, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「一大早就不见你的人影，\n"
"\\N[27]　我还担心你是不是被人拐跑了呢！」"
msgstr ""

#. ID 4, Page 1, Line 61, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……你说什么。森林里面有个洞穴？\n"
"\\N[27]　而且里面还有危险的生物？」"
msgstr ""

#. ID 4, Page 1, Line 64, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……什么，你想去里面调查？\n"
"\\N[27]　不行不行！　那种事情怎么可能让你去」"
msgstr ""

#. ID 4, Page 1, Line 67, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「危险的事情就交给我们负责吧，\n"
"\\N[27]　你只要专注在自己的学业上就行了」"
msgstr ""

#. ID 4, Page 1, Line 70, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「最近的年轻贵族如果涵养稍有欠缺，\n"
"\\N[27]　在贵族中就无法受到欢迎。\n"
"\\N[27]　唉，真是个麻烦的时代」"
msgstr ""

#. ID 4, Page 2, Line 4, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「提前排除城镇的潜在威胁\n"
"\\N[27]　也是我等领主一族的职责。\n"
"\\N[27]　你可要好好完成啊」"
msgstr ""

#. ID 4, Page 2, Line 10, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「可别去干危险的事情啊。\n"
"\\N[27]　真是的，你这淘气劲\n"
"\\N[27]　到了几岁才能过去啊」"
msgstr ""

#. ID 4, Page 3, Line 3, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「即使是什么怪物，只要是肉长的，\n"
"\\N[27]　也就跟盗匪没有分别」"
msgstr ""

#. ID 4, Page 3, Line 7, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「给我向边境警备队传令！\n"
"\\N[27]　火速召集自警团！\n"
"\\N[27]　组成队列，把怪物赶出城外！」"
msgstr ""

#. ID 4, Page 4, Line 5, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……虽然不能再让士兵继续牺牲，\n"
"\\N[27]　但在这里退缩会辱没家族之名啊」"
msgstr ""

#. ID 4, Page 4, Line 8, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[\\V[2]]啊，就由你来负责吧。\n"
"\\N[27]　你要一边锻炼实力，一边组织\n"
"\\N[27]　探险家们查出这场异变的原因」"
msgstr ""

#. ID 4, Page 4, Line 12, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「但是，你是霍尔姆领的后继人。\n"
"\\N[27]　我不允许你轻易死去」"
msgstr ""

#. ID 4, Page 4, Line 17, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……既然事已至此，别无选择了。\n"
"\\N[27]　虽然已经让士兵撤退，但事关\n"
"\\N[27]　家族名誉，我们还不能就此抽手」"
msgstr ""

#. ID 4, Page 4, Line 21, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[\\V[2]]啊，\n"
"\\N[27]　我就允许你参加探索吧。\n"
"\\N[27]　但是！仅限在安全的地区」"
msgstr ""

#. ID 4, Page 4, Line 25, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「没什么大不了，你就稍微作个参加\n"
"\\N[27]　的样子去露露面就行了。稍微地」"
msgstr ""

#. ID 4, Page 4, Line 32, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「我可是对你们的本事充满期待啊，\n"
"\\N[27]　去驱散弥漫在涅斯公国的阴云吧」"
msgstr ""

#. ID 4, Page 5, Line 4, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「击败地下宫殿中不死怪物\n"
"\\N[27]　的勇者就是你们吗。\n"
"\\N[27]　干得漂亮！」"
msgstr ""

#. ID 4, Page 5, Line 8, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「但是，灾祸的原因\n"
"\\N[27]　却依然没能被发现。\n"
"\\N[27]　请你们继续探索，找出其中根源」"
msgstr ""

#. ID 4, Page 5, Line 15, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「\\N[130]殿下思量霍尔姆的受灾情况，\n"
"\\N[27]　特地为我们筹备了谷粮等物资的援助」"
msgstr ""

#. ID 4, Page 5, Line 18, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「他是我们城镇的大恩人。\n"
"\\N[27]　你可千万别对他失礼了」"
msgstr ""

#. ID 4, Page 5, Line 23, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「森林北侧的不少村落\n"
"\\N[27]　都发来了求救的联络。\n"
"\\N[27]　似乎是被那些怪物袭击了」"
msgstr ""

#. ID 4, Page 5, Line 28, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「我虽然已经派兵前往，但人手始终不够。\n"
"\\N[27]　希望你也能去确认一下领内村落的安危」"
msgstr ""

#. ID 4, Page 5, Line 33, Pos (15,7)
msgid ""
"\\N[27]※这个委托没有日数的限制。\n"
"\\N[27]　不会随着时间经过陷入不利局面。"
msgstr ""

#. ID 4, Page 6, Line 5, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「我等也是信奉大河女神的信徒，\n"
"\\N[27]　神殿军应该不会对我们不利……」"
msgstr ""

#. ID 4, Page 6, Line 10, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「不过为什么大神殿的人，\n"
"\\N[27]　会对这个城镇报有兴趣……？」"
msgstr ""

#. ID 4, Page 6, Line 16, Pos (15,7)
msgid "\\N[27]卡穆罗似乎在考虑着事情。"
msgstr ""

#. ID 4, Page 6, Line 20, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「…………！？\n"
"\\N[27]　\\N[\\V[2]]……\n"
"\\N[27]　你果然还活着啊！」"
msgstr ""

#. ID 4, Page 6, Line 25, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「我的孩子，终于平平安安地回家了……\n"
"\n"
"\\N[27]　女神啊，感谢您的仁慈……！」"
msgstr ""

#. ID 4, Page 6, Line 31, Pos (15,7)
msgid "\\N[27]卡穆罗紧紧地抱住了你。"
msgstr ""

#. ID 4, Page 6, Line 33, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「但是，居然半年都没回家，\n"
"\\N[27]　你到底跑到哪里做什么去了？\n"
"\\N[27]　你这……你这个，不肖子啊！」"
msgstr ""

#. ID 4, Page 6, Line 39, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「你确实是那个……？\n"
"\\N[27]　听说你有一天潜入地下后，\n"
"\\N[27]　就再也没有传出消息了啊」"
msgstr ""

#. ID 4, Page 6, Line 43, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「原来你还活着啊。\n"
"\\N[27]　真让人感到欣喜」"
msgstr ""

#. ID 4, Page 6, Line 50, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……在你不在的这半年，\n"
"\\N[27]　霍尔姆发生了很大的变化」"
msgstr ""

#. ID 4, Page 6, Line 53, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「详细情况去问泽派克吧」"
msgstr ""

#. ID 4, Page 8, Line 5, Pos (15,7)
msgid ""
"\\N[27]卡穆罗在城镇被袭的战斗中负伤，被敌军囚禁，\n"
"\\N[27]直到前日才被解救出来。"
msgstr ""

#. ID 4, Page 8, Line 8, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……\\N[\\V[2]]啊。\n"
"\n"
"\\N[27]　嗯，我不要紧」"
msgstr ""

#. ID 4, Page 8, Line 12, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「我没有受到敌人太粗暴的对待，\n"
"\\N[27]　也好好地接受过治疗了」"
msgstr ""

#. ID 4, Page 8, Line 15, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「但是，接下来还有仗要打。\n"
"\\N[27]　我要亲率士兵奔赴战场了。\n"
"\\N[27]　你我二人，要是都能平安就好了啊……」"
msgstr ""

#. ID 4, Page 8, Line 19, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……\\N[\\V[2]]。\n"
"\\N[27]　稍微去外面走走吧。\n"
"\\N[27]　我有些话想对你讲」"
msgstr ""

#. ID 4, Page 8, Line 29, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……我们的故乡，真是个不错的地方啊」"
msgstr ""

#. ID 4, Page 8, Line 31, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「自从我在父亲那里继承了领地，\n"
"\\N[27]　就一直费尽心机，想把这里发展\n"
"\\N[27]　成更好的地方」"
msgstr ""

#. ID 4, Page 8, Line 35, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「为此，我做过玷污双手的事情。\n"
"\\N[27]　也做过会被人怀恨的事情」"
msgstr ""

#. ID 4, Page 8, Line 38, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「但是，也并非全是坏事。像刚刚继承领\n"
"\\N[27]　地的时候，还有在战争中取胜，夺回了\n"
"\\N[27]　霍尔姆和平的时候，我都发自内心地高兴」"
msgstr ""

#. ID 4, Page 8, Line 42, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「只不过，这一次……」"
msgstr ""

#. ID 4, Page 8, Line 46, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……在被囚期间，\n"
"\\N[27]　巴鲁斯穆斯曾对我讲过一席话，\n"
"\\N[27]　让我非常在意」"
msgstr ""

#. ID 4, Page 8, Line 53, Pos (15,7)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「DD卡穆罗卿。\n"
"\\N[27]　您是否了解\\N[130]公子\n"
"\\N[27]　的目的究竟为何？」"
msgstr ""

#. ID 4, Page 8, Line 58, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……？\n"
"\\N[27]　为什么问我这个」"
msgstr ""

#. ID 4, Page 8, Line 62, Pos (15,7)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「……在大河神殿的总部，\n"
"\\N[27]　有一页名为\\C[5]神帝记\\C[0]的古\n"
"\\N[27]　书的断章世代流传着」"
msgstr ""

#. ID 4, Page 8, Line 66, Pos (15,7)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「此书记载了有关阿尔凯亚初代\n"
"\\N[27]　皇帝的事迹。书中有一部分，\n"
"\\N[27]　还记述了皇帝复活的预言」"
msgstr ""

#. ID 4, Page 8, Line 71, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……那、究竟意味着什么」"
msgstr ""

#. ID 4, Page 8, Line 74, Pos (15,7)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「阿尔凯亚的初代皇帝将自己的灵魂\n"
"\\N[27]　存于异世，并通过附身于现世人类，\n"
"\\N[27]　造就了实质上的不死之身……」"
msgstr ""

#. ID 4, Page 8, Line 78, Pos (15,7)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「卡穆罗卿。\n"
"\\N[27]　你们将古代遗迹揭露于世，\n"
"\\N[27]　就等同于唤醒了古代皇帝的灵魂」"
msgstr ""

#. ID 4, Page 8, Line 86, Pos (15,7)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「他为了复活，必须要找到能满足\n"
"\\N[27]　特定条件的人类躯体，以附身其上」"
msgstr ""

#. ID 4, Page 8, Line 89, Pos (15,7)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「那条件即为，继承了阿尔凯亚皇室血裔，\n"
"\\N[27]　作为孤儿在大河岸边被拣起并抚养长大，\n"
"\\N[27]　并且达成与皇帝同等的伟业……」 "
msgstr ""

#. ID 4, Page 8, Line 94, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……！」"
msgstr ""

#. ID 4, Page 8, Line 97, Pos (15,7)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「因此，我认为……\n"
"\\N[27]　\\N[130]公子便是预言\n"
"\\N[27]　提及的古代皇帝后继者」"
msgstr ""

#. ID 4, Page 8, Line 101, Pos (15,7)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「如果事情照他的期望发展下去，\n"
"\\N[27]　终有一日他会以重振帝国为目标，\n"
"\\N[27]　将整个世界卷入战乱之中吧」"
msgstr ""

#. ID 4, Page 8, Line 105, Pos (15,7)
msgid ""
"\\N[27]巴鲁斯穆斯 \n"
"\\N[27]「即便只为了防止此事，我等神殿军\n"
"\\N[27]　也必须彻底将这里的遗迹封锁。\n"
"\\N[27]　我衷心希望您也能来协助我们」"
msgstr ""

#. ID 4, Page 8, Line 111, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「虽然也可以将这番话当成\n"
"\\N[27]　离间之计，对此一笑置之。\n"
"\\N[27]　但是，我却始终无法做到……」"
msgstr ""

#. ID 4, Page 8, Line 115, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……\\N[\\V[2]]。\n"
"\\N[27]　从今以后，霍尔姆恐怕要战乱不断了」"
msgstr ""

#. ID 4, Page 8, Line 118, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「但是，这也与你无关。\n"
"\\N[27]　你就和朋友一起离开吧，\n"
"\\N[27]　没有必要牵扯其中」"
msgstr ""

#. ID 4, Page 8, Line 122, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「因为，你其实……\n"
"\\N[27]　不是我亲生的」"
msgstr ""

#. ID 4, Page 8, Line 125, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「在过去……刚刚遭受了丧子之痛的\n"
"\\N[27]　我的妻子，在大河岸边捡起一个婴儿」"
msgstr ""

#. ID 4, Page 8, Line 128, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「我妻子说，婴儿的你裸着身子漂到岸边。\n"
"\\N[27]　水鸟和山狗像是在守护你一样紧随左右，\n"
"\\N[27]　在它们中间，你露出天使般的笑容……」"
msgstr ""

#. ID 4, Page 8, Line 132, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……从今往后，不管发生什么，\n"
"\\N[27]　我都会作为霍尔姆的伯爵来面对」"
msgstr ""

#. ID 4, Page 8, Line 135, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「但是，你并没有继承霍尔姆的血统，\n"
"\\N[27]　不需要被霍尔姆一族的名誉束缚」"
msgstr ""

#. ID 4, Page 8, Line 139, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「你没有需要肩负责任的理由。\n"
"\\N[27]　也不需要勉强自己战斗。你只要\n"
"\\N[27]　按自己的想法自由地活下去就好」"
msgstr ""

#. ID 4, Page 8, Line 145, Pos (15,7)
msgid ""
"\\N[27]只留下这些话，卡穆罗就为了备战，\n"
"\\N[27]而匆匆地赶回城馆中去了。"
msgstr ""

#. ID 4, Page 8, Line 148, Pos (15,7)
msgid ""
"\\N[27]刚才在你身后静静看着的\\N[11]，\n"
"\\N[27]这时走过来想要说些安慰你的话。"
msgstr ""

#. ID 4, Page 8, Line 151, Pos (15,7)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「\\N[\\V[2]]……」"
msgstr ""

#. ID 4, Page 8, Line 153, Pos (15,7)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「那个……\n"
"\\N[27]　我也、我也得去参加作战会议才行」"
msgstr ""

#. ID 4, Page 8, Line 157, Pos (15,7)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「该怎么说好、那个……\n"
"\\N[27]　请你保重！」"
msgstr ""

#. ID 4, Page 8, Line 162, Pos (15,7)
msgid ""
"\\N[27]结果\\N[11]也只是，带着一副\n"
"\\N[27]欲言又止的表情转身离开了。"
msgstr ""

#. ID 4, Page 8, Line 171, Pos (15,7)
msgid ""
"\\N[27]\\N[14]\n"
"\\N[27]「…………」"
msgstr ""

#. ID 4, Page 8, Line 175, Pos (15,7)
msgid ""
"\\N[27]等你注意到的时候，\\N[14]的身影\n"
"\\N[27]也消失得无影无踪。\n"
"\\N[27]他似乎一语不发地离去了。"
msgstr ""

#. ID 4, Page 8, Line 181, Pos (15,7)
msgid ""
"\\N[27]今后，你可以通过调查城外霍尔姆的军队来参加战争。\n"
"\\N[27]一旦战争开始，在全部结束之前都将无法返回城镇。"
msgstr ""

#. ID 4, Page 8, Line 192, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「……\\N[130]公子沉溺在自己的野心当中」"
msgstr ""

#. ID 4, Page 8, Line 194, Pos (15,7)
msgid ""
"\\N[27]卡穆罗\n"
"\\N[27]「我所热爱的霍尔姆，\n"
"\\N[27]　只怕也会被席卷其中了……」"
msgstr ""

#. ID 6, Page 4, Line 3, Pos (17,13)
#. Choice (2 options)
msgid ""
"\\N[27]关于休息\n"
"\\N[27]返回"
msgstr ""

#. ID 6, Page 4, Line 5, Pos (17,13)
msgid ""
"\\N[27]※在自家等地按下休息图标，就能恢复体力，\n"
"\\N[27]　并进入第二天。　\n"
"\\N[27]　以较少的日数使事态进展，也许就会碰上\n"
"\\N[27]　什么好事情。"
msgstr ""

#. ID 6, Page 4, Line 9, Pos (17,13)
msgid ""
"\\N[27]※但是同伴的变更、采取、一部分物品的购买\n"
"\\N[27]　在同一天当中有着数量的限制。"
msgstr ""

#. ID 8, Page 1, Line 4, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「哦呀，\\N[\\V[2]]大人。\n"
"\\N[27]　您是要出门吗？」"
msgstr ""

#. ID 8, Page 1, Line 7, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「年轻人就应该稍微做些莽撞无谋之事，\n"
"\\N[27]　馆主大人那边由我来蒙混过去，您慢走」"
msgstr ""

#. ID 8, Page 1, Line 12, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「领主大人向来多忙。\n"
"\\N[27]　如果没有什么要紧事情的话，\n"
"\\N[27]　大人是不会与你们相见的」"
msgstr ""

#. ID 8, Page 2, Line 4, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「唉呀唉呀，虽然不知道详情，\n"
"\\N[27]　不过看来有的忙了啊」"
msgstr ""

#. ID 8, Page 3, Line 5, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「\\N[13]哟，你有好好地\n"
"\\N[27]　成为\\N[\\V[2]]大人的助力吗？」"
msgstr ""

#. ID 8, Page 3, Line 10, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「哦呀，\\N[13]。\n"
"\\N[27]　你这段时间的表现不错啊」"
msgstr ""

#. ID 8, Page 3, Line 16, Pos (11,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「爷、爷爷……」"
msgstr ""

#. ID 8, Page 3, Line 19, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「想必也会碰到凶险之事吧，千万要小心。\n"
"\\N[27]　要是我再年轻一些，处在责任更少一些\n"
"\\N[27]　的立场的话，肯定也会跟你一起去的」"
msgstr ""

#. ID 8, Page 3, Line 26, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「\\N[\\V[2]]大人，请您务必小心。\n"
"\\N[27]　卡穆罗大人心里惦记着您呢」"
msgstr ""

#. ID 8, Page 3, Line 31, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「公国的大公陛下曾亲口约定，\n"
"\\N[27]　查明异变真相的人，将被授予\n"
"\\N[27]　丰厚赏赐」"
msgstr ""

#. ID 8, Page 4, Line 4, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「听说涅斯的公子殿下在十几岁的时候，\n"
"\\N[27]　是个拉帮结派在闹市中游荡的花花公子」"
msgstr ""

#. ID 8, Page 4, Line 7, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「我原本担心他的为人……\n"
"\\N[27]　但亲眼见过后发现他与传闻不同，\n"
"\\N[27]　是个让人有好感的年轻人」"
msgstr ""

#. ID 8, Page 4, Line 13, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「我们一族居住的雷恩蒂姆村，\n"
"\\N[27]　似乎也遭受到了怪物的袭击」"
msgstr ""

#. ID 8, Page 4, Line 16, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「乡里有很多本事不错的人，所以\n"
"\\N[27]　我觉得应该不会出什么事……\n"
"\\N[27]　但不知为什么，总觉得胸口闷闷的……」"
msgstr ""

#. ID 8, Page 4, Line 22, Pos (11,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……爷爷……」"
msgstr ""

#. ID 8, Page 5, Line 4, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「\\N[\\V[2]]大人，\n"
"\\N[27]　您果然活着回来了啊！\n"
"\\N[27]　哎呀，真是太值得庆幸了！」"
msgstr ""

#. ID 8, Page 5, Line 10, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「哎呀？\n"
"\\N[27]　我听说，你已经失踪了很久……」"
msgstr ""

#. ID 8, Page 5, Line 13, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「原来还活着啊。\n"
"\\N[27]　真是让人欣慰」"
msgstr ""

#. ID 8, Page 5, Line 20, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「在一起的这不是\\N[13]吗！\n"
"\\N[27]　你也平安无事啊，太好了……」"
msgstr ""

#. ID 8, Page 5, Line 24, Pos (11,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「爷爷……\n"
"\\N[27]　对不起，让您这么担心」"
msgstr ""

#. ID 8, Page 5, Line 33, Pos (11,9)
msgid "\\N[27]泽派克悄悄压低了声音。"
msgstr ""

#. ID 8, Page 5, Line 35, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「……但是，在这半年时间，\n"
"\\N[27]　镇上变得很奇怪了」"
msgstr ""

#. ID 8, Page 5, Line 38, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「镇上又是翻译古书进行再出版，\n"
"\\N[27]　又是仿制古代风格的绘画和雕塑，\n"
"\\N[27]　古代潮流已经在全国引起轰动」"
msgstr ""

#. ID 8, Page 5, Line 42, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「打起旗号说什么要将\n"
"\\N[27]　古代的智慧再次复兴……\n"
"\\N[27]　真是莫名其妙」"
msgstr ""

#. ID 8, Page 5, Line 46, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「明明怪物的灾害都没有消失，\n"
"\\N[27]　各地流行的怪病也没有好转，\n"
"\\N[27]　大家却已经把探索的目的忘得一干二净」"
msgstr ""

#. ID 8, Page 5, Line 50, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「现在的霍尔姆，已经变成了\n"
"\\N[27]　盗崛者的乐园，他们只顾着\n"
"\\N[27]　挖掘出古代遗物去倒卖……」"
msgstr ""

#. ID 8, Page 5, Line 55, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「而且，一手推动这种风潮的人\n"
"\\N[27]　是\\N[130]殿下自己。\n"
"\\N[27]　我们也无法劝阻他的决定」"
msgstr ""

#. ID 8, Page 6, Line 4, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「由我来告诉您如今世界的形势吧？\n"
"\\N[27]　想与来自国外人士交流的话，\n"
"\\N[27]　掌握这些是必要的」"
msgstr ""

#. ID 8, Page 6, Line 10, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「由我来告诉你们如今世界的形势吧？"
msgstr ""

#. ID 8, Page 6, Line 26, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「我们所在的霍尔姆伯爵领，\n"
"\\N[27]　就位于涅斯公国的西南之隅」"
msgstr ""

#. ID 8, Page 6, Line 31, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「涅斯公国以大公为国家君主，\n"
"\\N[27]　是由众多诸侯协力建成的国家」"
msgstr ""

#. ID 8, Page 6, Line 36, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「与公国在西方相临的，\n"
"\\N[27]　是西席瓦尔国的领地」"
msgstr ""

#. ID 8, Page 6, Line 39, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「直到三十年前为止，\n"
"\\N[27]　西席瓦尔和我们涅斯都是敌对关系。\n"
"\\N[27]　但是如今已经促成了和平」"
msgstr ""

#. ID 8, Page 6, Line 43, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「在很久以前，西席瓦尔与DD」"
msgstr ""

#. ID 8, Page 6, Line 47, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「艾鲁帕迪亚DD」"
msgstr ""

#. ID 8, Page 6, Line 51, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「还有我们涅斯公国，\n"
"\\N[27]　曾经同属于一个叫做\n"
"\\N[27]　大席瓦尔的国家」"
msgstr ""

#. ID 8, Page 6, Line 55, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「但是大席瓦尔王国在建国后不久\n"
"\\N[27]　就分裂成了三个国家，那时留下\n"
"\\N[27]　来的动乱直到现在都还持续着」"
msgstr ""

#. ID 8, Page 6, Line 61, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「与西席瓦尔相邻的这里，是大神殿\n"
"\\N[27]　总部所在的尤鲁弗莱尔岛」"
msgstr ""

#. ID 8, Page 6, Line 64, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「这座尤鲁弗莱尔岛，\n"
"\\N[27]　是神殿为了与异教徒战斗\n"
"\\N[27]　而建立的神殿军的大本营」"
msgstr ""

#. ID 8, Page 6, Line 68, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「大神殿是对大河流域各国\n"
"\\N[27]　有着绝大影响力的组织。\n"
"\\N[27]　他们的势力绝对不容忽视」"
msgstr ""

#. ID 8, Page 6, Line 72, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「最近大神殿似乎与\n"
"\\N[27]　西席瓦尔日渐亲密。\n"
"\\N[27]　这对涅斯公国来说不是什么好消息」"
msgstr ""

#. ID 8, Page 6, Line 76, Pos (11,9)
msgid ""
"\\N[27]西席瓦尔\n"
"\\N[27]「大致就是这些了」"
msgstr ""

#. ID 8, Page 8, Line 5, Pos (11,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……爷爷你平安无事，\n"
"\\N[27]　真是太好了」"
msgstr ""

#. ID 8, Page 8, Line 9, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「啊啊。虽然受了很重的伤，\n"
"\\N[27]　但我这条老命还硬朗着呢」"
msgstr ""

#. ID 8, Page 8, Line 12, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「不过辅佐卡穆罗大人去参加战争的话，\n"
"\\N[27]　这以后会变成怎样就不好说了」"
msgstr ""

#. ID 8, Page 8, Line 16, Pos (11,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「…………」"
msgstr ""

#. ID 8, Page 8, Line 19, Pos (11,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「别摆出那种表情。\n"
"\\N[27]　要是有个万一你就到林迪姆乡\n"
"\\N[27]　去找古斯坦吧」"
msgstr ""

#. ID 8, Page 8, Line 23, Pos (11,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「那孩子虽然很可靠，但是为人有些冲动。\n"
"\\N[27]　你要多在旁边为他出出主意」"
msgstr ""

#. ID 8, Page 8, Line 27, Pos (11,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「是……」"
msgstr ""

#. ID 8, Page 8, Line 31, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「虽然受过重伤，但我这条老命还硬朗呢」"
msgstr ""

#. ID 8, Page 8, Line 33, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「不过之后我还要辅佐卡穆罗大人\n"
"\\N[27]　去参加战争，\n"
"\\N[27]　这以后的事情就不好说了……」"
msgstr ""

#. ID 8, Page 8, Line 43, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「还有，\\N[\\V[2]]大人……\n"
"\\N[27]　关于您自己的出生，\n"
"\\N[27]　您已经从领主大人那里听说了吧」"
msgstr ""

#. ID 8, Page 8, Line 47, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「卡穆罗大人一直把\\N[\\V[2]]大人\n"
"\\N[27]　当作自己亲生的孩子一样，\n"
"\\N[27]　关怀得无微不至，这点我可以保证」"
msgstr ""

#. ID 8, Page 9, Line 4, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「……在三十年前的战争中，\n"
"\\N[27]　馆主大人豁出性命，\n"
"\\N[27]　保住了不过是一介败军之将的我……」"
msgstr ""

#. ID 8, Page 9, Line 9, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「DD馆主大人最后的遗愿，\n"
"\\N[27]　是希望这片土地能重新取回和平」"
msgstr ""

#. ID 8, Page 9, Line 12, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「请务必，实现大人的这份心愿」"
msgstr ""

#. ID 8, Page 9, Line 16, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「领主卡穆罗大人，是在战场上被从\n"
"\\N[27]　背后袭来的利刃刺杀而亡的」"
msgstr ""

#. ID 8, Page 9, Line 19, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「这是谁下的手。\n"
"\\N[27]　又是由谁下的命令……\n"
"\\N[27]　我虽然知道，但却说不得」"
msgstr ""

#. ID 8, Page 10, Line 4, Pos (11,9)
#. ID 8, Page 10, Line 27, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「城镇的复兴非常顺利。现在最要紧的，\n"
"\\N[27]　是该如何处理新兴的贫民街」"
msgstr ""

#. ID 8, Page 10, Line 7, Pos (11,9)
#. ID 8, Page 10, Line 30, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「要做的事情太多了，\n"
"\\N[27]　连我这个老脑袋都疼起来了……」"
msgstr ""

#. ID 8, Page 10, Line 10, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「但这毕竟是我们的职责。\n"
"\\N[27]　就让我勉力而为吧」"
msgstr ""

#. ID 8, Page 10, Line 16, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「你要出发了吗，\\N[13]」"
msgstr ""

#. ID 8, Page 10, Line 19, Pos (11,9)
#. ID 12, Page 9, Line 262, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「是的……」"
msgstr ""

#. ID 8, Page 10, Line 22, Pos (11,9)
msgid ""
"\\N[27]泽派克\n"
"\\N[27]「是嘛……\n"
"\\N[27]　唔，要多保重身体」"
msgstr ""

#. ID 12, Page 1, Line 4, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「卡穆罗领主的夫人\n"
"\\N[27]　在大前年去世了……\n"
"\\N[27]　那之后的领主大人一直很寂寞」"
msgstr ""

#. ID 12, Page 2, Line 3, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「\\N[\\V[2]]大人\n"
"\\N[27]　请您不要太冒险了」"
msgstr ""

#. ID 12, Page 2, Line 7, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「要是觉得累了就请休息吧，\n"
"\\N[27]　请千万不要太过勉强自己，\n"
"\\N[27]　要是有个万一该如何是好」"
msgstr ""

#. ID 12, Page 2, Line 14, Pos (6,9)
msgid ""
"\\N[27]※按下右下方的「休息图标」，\n"
"\\N[27]　就可以进行体力的恢复。"
msgstr ""

#. ID 12, Page 3, Line 4, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「\\N[\\V[2]]大人，\n"
"\\N[27]　请一定要平安无事」"
msgstr ""

#. ID 12, Page 3, Line 9, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「各位，没有受伤吧？」"
msgstr ""

#. ID 12, Page 5, Line 2, Pos (6,9)
msgid ""
"\\N[27]骑士\n"
"\\N[27]「…………」"
msgstr ""

#. ID 12, Page 5, Line 4, Pos (6,9)
msgid ""
"\\N[27]骑士\n"
"\\N[27]「你盯着我看什么看啊？」"
msgstr ""

#. ID 12, Page 9, Line 43, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……城镇周边的夜种讨伐\n"
"\\N[27]　都进展得很顺利」"
msgstr ""

#. ID 12, Page 9, Line 46, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「照这种步调进行下去，很快周围的村落\n"
"\\N[27]　就能恢复平静，农民们也都可以安心地\n"
"\\N[27]　返回农田了」"
msgstr ""

#. ID 12, Page 9, Line 55, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「而且我们还有\\N[\\V[2]]大人，\n"
"\\N[27]　由您作为领主来统治这块土地，\n"
"\\N[27]　我们大家都很放心」"
msgstr ""

#. ID 12, Page 9, Line 60, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……话说回来\\N[\\V[2]]大人。\n"
"\\N[27]　您准备什么时候正式继任\n"
"\\N[27]　霍尔姆伯爵的地位呢？」"
msgstr ""

#. ID 12, Page 9, Line 67, Pos (6,9)
#. Choice (2 options)
msgid ""
"没有那种打算\n"
"就在最近"
msgstr ""

#. ID 12, Page 9, Line 70, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「诶……！？」"
msgstr ""

#. ID 12, Page 9, Line 73, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「不想继承霍尔姆伯爵……\n"
"\\N[27]　您真是这个意思吗？」"
msgstr ""

#. ID 12, Page 9, Line 82, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「为什么……？」"
msgstr ""

#. ID 12, Page 9, Line 84, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……不行啊，您不能这样……」"
msgstr ""

#. ID 12, Page 9, Line 88, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「您可是，大家的……\n"
"\\N[27]　我的寄托啊！\n"
"\\N[27]　您是我的希望！」"
msgstr ""

#. ID 12, Page 9, Line 92, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「并不只是因为您拯救了镇子。\n"
"\\N[27]　我一直在背后看着\\N[\\V[2]]大人奋战，\n"
"\\N[27]　一直为您那种身姿而感到鼓舞不已啊……」"
msgstr ""

#. ID 12, Page 9, Line 97, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「请不要把大家的期待……责任……\n"
"\\N[27]　请不要舍弃我们大家！！」"
msgstr ""

#. ID 12, Page 9, Line 103, Pos (6,9)
#. Contains choice at line 3 (2 options)
msgid ""
"\\N[27]\\N[13]少见地激烈倾诉着自己的感情。\n"
"\\N[27]激烈到了语言无法承载起全部感情的程度。"
msgstr ""

#. ID 12, Page 9, Line 106, Pos (6,9)
#. Choice (2 options, embedded in a message)
msgid ""
"把她抱到怀里\n"
"离去"
msgstr ""

#. ID 12, Page 9, Line 110, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「啊……！？」"
msgstr ""

#. ID 12, Page 9, Line 114, Pos (6,9)
msgid ""
"\\N[27]虽然在狼狈地挣扎，但并没有挣脱，\n"
"\\N[27]她在你的怀抱中渐渐安静了下来。"
msgstr ""

#. ID 12, Page 9, Line 116, Pos (6,9)
msgid "\\N[27]终于，\\N[13]发出了耳语一般的细微之声。"
msgstr ""

#. ID 12, Page 9, Line 119, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……我知道的啊……\n"
"\\N[27]　我只是，在由着自己的性子\n"
"\\N[27]　说些任性的话而已」"
msgstr ""

#. ID 12, Page 9, Line 123, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「我只是、只是想要\n"
"\\N[27]　和\\N[\\V[2]]大人在一起而已……」"
msgstr ""

#. ID 12, Page 9, Line 126, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「您总有一天……会到别的什么地方去。\n"
"\\N[27]　我从前就一直有着这种预感」"
msgstr ""

#. ID 12, Page 9, Line 130, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「至少……请再稍微，\n"
"\\N[27]　让我在您的身旁多待一会……」"
msgstr ""

#. ID 12, Page 9, Line 135, Pos (6,9)
msgid "\\N[27]\\N[13]轻轻呢喃道。"
msgstr ""

#. ID 12, Page 9, Line 143, Pos (6,9)
msgid ""
"\\N[27]\\N[13]一边泣不成声地低语着，\n"
"\\N[27]一边靠到了你的身上……"
msgstr ""

#. ID 12, Page 9, Line 157, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「这样啊……\n"
"\\N[27]　真是太好了」"
msgstr ""

#. ID 12, Page 9, Line 162, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……我还担心您会不会因为在意出身，\n"
"\\N[27]　就想要舍弃霍尔姆伯爵的爵位呢……」"
msgstr ""

#. ID 12, Page 9, Line 165, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「\\N[\\V[2]]大人。\n"
"\\N[27]　您是这片土地的正当领主，\n"
"\\N[27]　是我的君主……」"
msgstr ""

#. ID 12, Page 9, Line 170, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「今后，也请让我从旁辅佐您吧」"
msgstr ""

#. ID 12, Page 9, Line 175, Pos (6,9)
#. Contains choice at line 3 (2 options)
msgid ""
"\\N[27]\\N[13]虽然露出一副想说什么的表情，\n"
"\\N[27]但最终没说出口，只对你展露出一个微笑。"
msgstr ""

#. ID 12, Page 9, Line 178, Pos (6,9)
#. Choice (2 options, embedded in a message)
msgid ""
"不想只是她的君主\n"
"今后也请多关照"
msgstr ""

#. ID 12, Page 9, Line 181, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「诶？\n"
"\\N[27]　您说什么……？」"
msgstr ""

#. ID 12, Page 9, Line 187, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「诶？　诶呀？\n"
"\n"
"\\N[27]　……呜诶诶诶！？」"
msgstr ""

#. ID 12, Page 9, Line 193, Pos (6,9)
msgid ""
"\\N[27]你把她抱入怀中，将话语的意义表露无遗后，\n"
"\\N[27]一抹赤红从\\N[13]的脖子一直延伸到了脸庞。"
msgstr ""

#. ID 12, Page 9, Line 196, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……这、这种事情、不行的。\n"
"\\N[27]　主从之间，不能轻易变成那种关系的」"
msgstr ""

#. ID 12, Page 9, Line 199, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「我只要能一直陪伴在您身边\n"
"\\N[27]　就觉得足够幸福了……\n"
"\\N[27]　这以上的关系……」"
msgstr ""

#. ID 12, Page 9, Line 205, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「请让我……\n"
"\\N[27]　请给我一点作心理准备的时间！」"
msgstr ""

#. ID 12, Page 9, Line 209, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「这这之前……必须要通知亲戚乡邻们，\n"
"\\N[27]　和他们提前打好招呼，啊啊，还必须\n"
"\\N[27]　准备好新娘用品和衣服还有嫁妆……！」"
msgstr ""

#. ID 12, Page 9, Line 215, Pos (6,9)
msgid "\\N[27]你一个不留神就被她先偷跑了！"
msgstr ""

#. ID 12, Page 9, Line 220, Pos (6,9)
msgid ""
"\\N[27]你靠近她，将自己的意思传达出去后，\n"
"\\N[27]\\N[13]好象很是迷惑。"
msgstr ""

#. ID 12, Page 9, Line 223, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「诶、那个、\n"
"\\N[27]　\\N[\\V[2]]大人……？」"
msgstr ""

#. ID 12, Page 9, Line 226, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「………………」"
msgstr ""

#. ID 12, Page 9, Line 230, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……对我来说，作为仆从\n"
"\\N[27]　侍奉\\N[\\V[2]]大人才是\n"
"\\N[27]　最自然的生活方式……」"
msgstr ""

#. ID 12, Page 9, Line 234, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「我将您当作靠山一般倚重，\n"
"\\N[27]　但这对\\N[\\V[2]]大人来说\n"
"\\N[27]　也许只是个负担……」"
msgstr ""

#. ID 12, Page 9, Line 238, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「我们明明一直是在一起长大的……\n"
"\\N[27]　……就像是亲密的姐妹那样」"
msgstr ""

#. ID 12, Page 9, Line 241, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「对不起……\\N[\\V[2]]大人。\n"
"\\N[27]　我却对此一直没有自觉」"
msgstr ""

#. ID 12, Page 9, Line 244, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「从今以后……我也会同样\n"
"\\N[27]　支持起\\N[\\V[2]]大人，\n"
"\\N[27]　作为朋友……也作为家人」"
msgstr ""

#. ID 12, Page 9, Line 249, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「请您就尽管倚赖我吧！」"
msgstr ""

#. ID 12, Page 9, Line 253, Pos (6,9)
msgid ""
"\\N[27]\\N[13]拍了拍小小的胸脯，\n"
"\\N[27]对你露出了亲密的笑容。"
msgstr ""

#. ID 12, Page 9, Line 266, Pos (6,9)
msgid "\\N[27]\\N[13]静静地向你行了一礼。"
msgstr ""

#. ID 12, Page 9, Line 272, Pos (6,9)
msgid "\\N[27]\\N[13]静静地微笑着。"
msgstr ""

#. ID 12, Page 9, Line 285, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……事情告一段落之后，\n"
"\\N[27]　我也有了闲暇，于是就想要\n"
"\\N[27]　考虑一下自己的将来了」"
msgstr ""

#. ID 12, Page 9, Line 289, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「我想回家乡去看看。\n"
"\\N[27]　也有在意弟弟的行踪，\n"
"\\N[27]　想要去找他的这种想法……」"
msgstr ""

#. ID 12, Page 9, Line 293, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「至尽为止，我都一直认为侍奉于\n"
"\\N[27]　霍尔姆伯爵是我理想的生活方式……」"
msgstr ""

#. ID 12, Page 9, Line 296, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「但自从领主大人去世之后，\n"
"\\N[27]　我却不知道该怎么办了……\n"
"\\N[27]　我想要试着离开城镇，好好考虑一下」"
msgstr ""

#. ID 12, Page 9, Line 306, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「说起来，\\N[\\V[2]]大人\n"
"\\N[27]　今后有什么预定吗？」"
msgstr ""

#. ID 12, Page 9, Line 312, Pos (6,9)
#. Choice (3 options)
msgid ""
"\\与N[13]一起走\n"
"守护这个镇\n"
"外出旅行"
msgstr ""

#. ID 12, Page 9, Line 316, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「诶？」"
msgstr ""

#. ID 12, Page 9, Line 318, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……呜诶诶诶！？」"
msgstr ""

#. ID 12, Page 9, Line 321, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「啊哒啊哒哒啊哒哒哒啊哒哒噢啊哒」\n"
"\\N[27]※译注：健次郎用北斗百烈拳时的叫声\n"
"\\N[27]※译注：啊哒与日语第一人称あたし音近"
msgstr ""

#. ID 12, Page 9, Line 327, Pos (6,9)
msgid "\\N[27]\\N[13]坏掉了。"
msgstr ""

#. ID 12, Page 9, Line 329, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「您要和啊哒哒啊哒人家一起来！？\n"
"\\N[27]　增、增滴吗！？」"
msgstr ""

#. ID 12, Page 9, Line 332, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「不不，绝对不是不愿意\n"
"\\N[27]　是很高兴没错但你说这话究竟\n"
"\\N[27]　有什么含义不没什么我什么都没说」"
msgstr ""

#. ID 12, Page 9, Line 337, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「虽虽虽虽虽虽然小女还不成器，\n"
"\\N[27]　今后还请您多多关、关哨！」"
msgstr ""

#. ID 12, Page 9, Line 342, Pos (6,9)
msgid ""
"\\N[27]\\N[13]一边发出怪声，\n"
"\\N[27]一边向你深深鞠了一躬。"
msgstr ""

#. ID 12, Page 9, Line 346, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「诶！？」"
msgstr ""

#. ID 12, Page 9, Line 348, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「您真的愿意和我一起来吗？」"
msgstr ""

#. ID 12, Page 9, Line 350, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「太好了……\n"
"\\N[27]　其实，要自己一个人外出\n"
"\\N[27]　我还是觉得有些不安」"
msgstr ""

#. ID 12, Page 9, Line 355, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「虽然小女还不成器，\n"
"\\N[27]　今后也请您多多关照了」"
msgstr ""

#. ID 12, Page 9, Line 360, Pos (6,9)
msgid "\\N[27]\\N[13]向你深深鞠了一躬。"
msgstr ""

#. ID 12, Page 9, Line 364, Pos (6,9)
#. ID 12, Page 9, Line 419, Pos (6,9)
msgid "\\N[27]\\N[\\V[81]]成为了同伴！"
msgstr ""

#. ID 12, Page 9, Line 370, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「这样啊……\n"
"\\N[27]　这真是太好了……」"
msgstr ""

#. ID 12, Page 9, Line 375, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「只要想着再回到这座城镇、\n"
"\\N[27]　就能见到\\N[\\V[2]]大人，\n"
"\\N[27]　我就能安心踏上旅程了」"
msgstr ""

#. ID 12, Page 9, Line 380, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「啊、不不不、没有什么\n"
"\\N[27]　别的意思……」"
msgstr ""

#. ID 12, Page 9, Line 389, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「诶！\n"
"\\N[27]　您、您要去哪里？」"
msgstr ""

#. ID 12, Page 9, Line 392, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……啊、原来还没有\n"
"\\N[27]　决定吗……」"
msgstr ""

#. ID 12, Page 9, Line 395, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「那个……如果愿意的话，\n"
"\\N[27]　能让我陪伴着您吗？」"
msgstr ""

#. ID 12, Page 9, Line 399, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「啊不、并没有什么别的意思、\n"
"\\N[27]　反正我也想不到什么明确的目的地……」"
msgstr ""

#. ID 12, Page 9, Line 409, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「是的！\n"
"\\N[27]　请让我跟随您吧」"
msgstr ""

#. ID 12, Page 9, Line 413, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「可能会给您添麻烦，\n"
"\\N[27]　还请多多关照了……」"
msgstr ""

#. ID 12, Page 9, Line 425, Pos (6,9)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「这、这样啊……\n"
"\\N[27]　是我的要求太任性了，\n"
"\\N[27]　十分抱歉……」"
msgstr ""

#. ID 16, Page 1, Line 2, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「现在的霍尔姆只是个普通的田舍乡镇，\n"
"\\N[27]　不过在三十年前这里可是战争的最前线」"
msgstr ""

#. ID 16, Page 1, Line 5, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「所以直到现在，附近的要塞里还有\n"
"\\N[27]　国境警备队在驻扎着。\n"
"\\N[27]　正所谓是国家的咽喉之处啊」"
msgstr ""

#. ID 16, Page 2, Line 2, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「我亲眼看到了。\n"
"\\N[27]　……那些怪物简直就像童话故事里\n"
"\\N[27]　描述的小鬼一样，它们在到处袭击」"
msgstr ""

#. ID 16, Page 3, Line 2, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「……之前在这里站岗的家伙\n"
"\\N[27]　已经死掉了」"
msgstr ""

#. ID 16, Page 3, Line 5, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「请你务必要替他报仇」"
msgstr ""

#. ID 16, Page 4, Line 2, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「\\N[130]殿下仪表堂堂，真是帅气啊」"
msgstr ""

#. ID 16, Page 4, Line 4, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「骑士中的骑士就是说他这种人吧」"
msgstr ""

#. ID 16, Page 5, Line 3, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「要是阿尔凯亚帝国复兴了的话，\n"
"\\N[27]　我们霍尔姆镇会变成什么样呢？\n"
"\\N[27]　很久以前这里似乎是帝国的首都……」"
msgstr ""

#. ID 16, Page 5, Line 7, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「领主大人又会被怎么安置呢？」"
msgstr ""

#. ID 16, Page 5, Line 13, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「虽然说这种话有些不应该，\n"
"\\N[27]　不过我对\\N[130]殿下刮目相看了」"
msgstr ""

#. ID 16, Page 5, Line 16, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「他面对那位大河神殿的战将，\n"
"\\N[27]　居然还能毫不退让。\n"
"\\N[27]　真不愧是大公之子啊」"
msgstr ""

#. ID 16, Page 5, Line 22, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「这句话我就只在这里讲……」"
msgstr ""

#. ID 16, Page 5, Line 24, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「前日，大河神殿的巴鲁斯穆斯大神官\n"
"\\N[27]　来威胁领主大人，让他把霍尔姆镇交出去」"
msgstr ""

#. ID 16, Page 5, Line 27, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「据说是因为那啥，遗迹的秘密？\n"
"\\N[27]　因为被我们公布，就引起了神殿的不满……」"
msgstr ""

#. ID 16, Page 5, Line 30, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「但是，\\N[130]殿下不顾威胁，\n"
"\\N[27]　干脆利落地拒绝了神殿的要求。\n"
"\\N[27]　真是帅气啊」"
msgstr ""

#. ID 16, Page 5, Line 38, Pos (9,9)
msgid "\\N[27]兵士压低了声音说到。"
msgstr ""

#. ID 16, Page 5, Line 39, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「\\N[130]殿下带来了一队自幼跟随他的\n"
"\\N[27]　被称为＜火车骑士团＞的骑士」"
msgstr ""

#. ID 16, Page 5, Line 42, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「据说他们都是没落士族的二子与\n"
"\\N[27]　三子之类的身份，原本与殿下\n"
"\\N[27]　就是幼年玩伴的关系」"
msgstr ""

#. ID 16, Page 5, Line 46, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「但老实说，他们给人的感觉很恶劣。\n"
"\\N[27]　总是以一副物主的姿态巡视我们镇子」"
msgstr ""

#. ID 16, Page 5, Line 49, Pos (9,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「……或许是我想太多了，但那些人\n"
"\\N[27]　该不会真的打算就驻兵不走了吧，\n"
"\\N[27]　是打算要篡夺我们的城镇吗」"
msgstr ""

#. ID 16, Page 8, Line 2, Pos (9,9)
msgid ""
"\\N[27]骑士\n"
"\\N[27]「在\\N[130]大人不在的当下，\n"
"\\N[27]　紧紧跟随着\\N[11]才是上策……\n"
"\\N[27]　但是，总觉得那位大人有些靠不住……」"
msgstr ""

#. ID 18, Page 1, Line 2, Pos (10,10)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「我明明都没有参加过实战……\n"
"\\N[27]　真的能行吗」"
msgstr ""

#. ID 20, Page 1, Line 2, Pos (8,10)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「虽然不是很清楚，但敌人好象入侵了！\n"
"\\N[27]　总算有机会一试我的身手了」"
msgstr ""

#. ID 22, Page 1, Line 14, Pos (11,5)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「嗯嗯，为了人民，为了公国\n"
"\\N[27]　我每天都在努力奋战哦~」"
msgstr ""

#. ID 22, Page 1, Line 19, Pos (11,5)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「他这个人有些怪怪的，经常会被别人\n"
"\\N[27]　误会，不过其实是个心怀大志的人，\n"
"\\N[27]　我一直都很崇拜他」"
msgstr ""

#. ID 22, Page 1, Line 50, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「嘛，这话题就先放到一边吧」"
msgstr ""

#. ID 22, Page 2, Line 2, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「余最近在收集古代遗迹的发掘品」"
msgstr ""

#. ID 22, Page 2, Line 9, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「现在余想要的是\\C[5]古代铜币\\C[0]。\n"
"\\N[27]　如果你身上有带着，\n"
"\\N[27]　余愿意出\\V[169]Ｇ来买取」"
msgstr ""

#. ID 22, Page 2, Line 16, Pos (11,5)
#. Choice (2 options)
msgid ""
"以(\\V[169]Ｇ)卖出古代铜币\n"
"不卖"
msgstr ""

#. ID 22, Page 2, Line 20, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哦……这上面刻着什么人的侧身像啊。\n"
"\\N[27]　是王的吗？　还是神的呢？不管是\n"
"\\N[27]　哪一位，这栩栩如生的雕刻着实出色」"
msgstr ""

#. ID 22, Page 2, Line 27, Pos (11,5)
msgid "\\N[27]你用\\V[169]Ｇ的价格卖出了古代铜币。"
msgstr ""

#. ID 22, Page 2, Line 42, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「现在余想要的是\\C[5]古代陶器\\C[0]。\n"
"\\N[27]　如果你身上有带着，\n"
"\\N[27]　余愿意出\\V[169]Ｇ来买取」"
msgstr ""

#. ID 22, Page 2, Line 49, Pos (11,5)
#. Choice (2 options)
msgid ""
"以(\\V[169]Ｇ)卖出古代陶器\n"
"不卖"
msgstr ""

#. ID 22, Page 2, Line 53, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「过去的器皿，还真是意外地朴素啊。\n"
"\\N[27]　但朴素确实也有朴素的风味。\n"
"\\N[27]　这就是所谓静素之美吗？"
msgstr ""

#. ID 22, Page 2, Line 60, Pos (11,5)
msgid "\\N[27]你用\\V[169]Ｇ的价格卖出了古代陶器。"
msgstr ""

#. ID 22, Page 2, Line 75, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「现在余想要的是\\C[5]古代雕像\\C[0]。\n"
"\\N[27]　如果你身上有带着，\n"
"\\N[27]　余愿意出\\V[169]Ｇ来买取」"
msgstr ""

#. ID 22, Page 2, Line 82, Pos (11,5)
#. Choice (2 options)
msgid ""
"以(\\V[169]Ｇ)卖出古代雕像\n"
"不卖"
msgstr ""

#. ID 22, Page 2, Line 86, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「这是何等精致的雕像……\n"
"\\N[27]　和这个一比，现在的神像\n"
"\\N[27]　简直就是顽童玩的泥巴！」"
msgstr ""

#. ID 22, Page 2, Line 93, Pos (11,5)
msgid "\\N[27]你用\\V[169]Ｇ的价格卖出了古代雕像。"
msgstr ""

#. ID 22, Page 2, Line 108, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「现在余想要的是\\C[5]古代文献\\C[0]。\n"
"\\N[27]　如果你身上有带着，\n"
"\\N[27]　余愿意出\\V[169]Ｇ来买取」"
msgstr ""

#. ID 22, Page 2, Line 115, Pos (11,5)
#. Choice (2 options)
msgid ""
"以(\\V[169]Ｇ)卖出古代书籍\n"
"不卖"
msgstr ""

#. ID 22, Page 2, Line 120, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「这是在石头……不，是在瓷器\n"
"\\N[27]　的碎片上篆刻的数字。\n"
"\\N[27]　应该是古代人作记录之用的」"
msgstr ""

#. ID 22, Page 2, Line 127, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「唉呀！\n"
"\\N[27]　这些纸张好象轻轻一碰就会碎掉啊。\n"
"\\N[27]　必须小心轻放才行」"
msgstr ""

#. ID 22, Page 2, Line 134, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哦？这快粘土版上记载的文章\n"
"\\N[27]　还保留得相当完整啊。\n"
"\\N[27]　搞不好会是什么重大发现」"
msgstr ""

#. ID 22, Page 2, Line 148, Pos (11,5)
msgid "\\N[27]你用\\V[169]Ｇ的价格卖出了古代文献。"
msgstr ""

#. ID 22, Page 2, Line 163, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「现在余想要的是\\C[5]古代银币\\C[0]。\n"
"\\N[27]　如果你身上有带着，\n"
"\\N[27]　余愿意出\\V[169]Ｇ来买取」"
msgstr ""

#. ID 22, Page 2, Line 170, Pos (11,5)
#. Choice (2 options)
msgid ""
"以(\\V[169]Ｇ)卖出古代银币\n"
"不卖"
msgstr ""

#. ID 22, Page 2, Line 174, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「这些古代的货币，\n"
"\\N[27]　大小重量都做得恰倒好处啊。\n"
"\\N[27]　这样就很方便用于交易了……」"
msgstr ""

#. ID 22, Page 2, Line 181, Pos (11,5)
msgid "\\N[27]你用\\V[169]Ｇ的价格卖出了古代银币。"
msgstr ""

#. ID 22, Page 3, Line 5, Pos (11,5)
#. Choice (3 options)
msgid ""
"交谈关于遗迹的话题\n"
"询问有关脸上的伤痕\n"
"出售物品"
msgstr ""

#. ID 22, Page 3, Line 7, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「据说在遗迹深处，有个被称为\n"
"\\N[27]　皇帝或是魔王的不死怪物」"
msgstr ""

#. ID 22, Page 3, Line 10, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「虽然镇上流传着，那东西就是诸恶\n"
"\\N[27]　的根源、灾祸的元凶这一类传言……」"
msgstr ""

#. ID 22, Page 3, Line 13, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「但那个皇帝究竟是什么人？\n"
"\\N[27]　他的目的又是什么？余对此颇有兴趣」"
msgstr ""

#. ID 22, Page 3, Line 18, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「想知道，余是怎么在脸上留下这条\n"
"\\N[27]  伤疤的？」"
msgstr ""

#. ID 22, Page 3, Line 21, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「这个啊，是在余幼年时，\n"
"\\N[27]　被父王DD涅斯大公刻下来的。\n"
"\\N[27]　为了让它时刻告戒余远离懦弱」"
msgstr ""

#. ID 22, Page 3, Line 25, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「余等涅斯一族是东方守护神狮子公的\n"
"\\N[27]　后裔。若是出现懦弱之辈，即为亲子\n"
"\\N[27]　也死不足惜，伤疤就是余的觉悟之证」"
msgstr ""

#. ID 22, Page 3, Line 31, Pos (11,5)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「…………」"
msgstr ""

#. ID 22, Page 4, Line 5, Pos (11,5)
#. Choice (3 options)
msgid ""
"交谈关于遗迹的话题\n"
"听取有关国家的形势\n"
"出售道具"
msgstr ""

#. ID 22, Page 4, Line 7, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「最近，有些嗅觉敏锐的商人把从这个\n"
"\\N[27]　遗迹发掘出的古代文物运到首都去卖。\n"
"\\N[27]　诸如雕像之类」"
msgstr ""

#. ID 22, Page 4, Line 11, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「余看到其中某件雕像时，忍不住浑身颤抖。\n"
"\\N[27]　那是一座栩栩如声的雕像，雕塑着一名\n"
"\\N[27]　仿佛下一秒就会冲出去砍杀敌人的战士」"
msgstr ""

#. ID 22, Page 4, Line 15, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「和那东西一比，现在神殿里摆设\n"
"\\N[27]　的雕像简直就如同顽童玩的泥巴。\n"
"\\N[27]　究竟是何等民族能如此巧夺天工……」"
msgstr ""

#. ID 22, Page 4, Line 19, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「那是余在幼年时曾听导师讲过的，\n"
"\\N[27]　传说中古英雄时代的产物吗……」"
msgstr ""

#. ID 22, Page 4, Line 22, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「想到这点，余就再也坐不住了，\n"
"\\N[27]　一定要来亲眼来确认这个事实」"
msgstr ""

#. ID 22, Page 4, Line 27, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「如今的涅斯公国，由六十四个小型\n"
"\\N[27]　邦国结成。代表涅斯权威的大公，\n"
"\\N[27]　只不过是其中的第一诸侯罢了」"
msgstr ""

#. ID 22, Page 4, Line 31, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「即使地方的贵族实施暴政\n"
"\\N[27]　压迫当地人民，大公也无权干涉。\n"
"\\N[27]　真荒唐，明明人民才是国家之本」"
msgstr ""

#. ID 22, Page 4, Line 35, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「大公打算以霍尔姆伯爵领发生的这场\n"
"\\N[27]　异变为契机，重新调整大公与各诸侯\n"
"\\N[27]　之间的关系」"
msgstr ""

#. ID 22, Page 4, Line 39, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「为此，身为公子的余必须要\n"
"\\N[27]　身先士卒才行」"
msgstr ""

#. ID 22, Page 5, Line 3, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「你原来还活着吗。\n"
"\\N[27]　哈哈哈，那真是不幸中的万幸啊」"
msgstr ""

#. ID 22, Page 5, Line 7, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「余还正想着，要为失去了嫡子的霍\n"
"\\N[27]　尔姆伯爵，筹办一下收养义子之事」"
msgstr ""

#. ID 22, Page 5, Line 10, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「现在这事也不用余来拿捏了。\n"
"\\N[27]　真是让人欣喜万分啊」"
msgstr ""

#. ID 22, Page 5, Line 16, Pos (11,5)
#. Choice (3 options)
msgid ""
"交谈关于遗迹的话题\n"
"听取有关外国的形势\n"
"出售道具"
msgstr ""

#. ID 22, Page 5, Line 18, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「结果很让人遗憾，尽管你打倒了\n"
"\\N[27]　怪物们的首领，灾祸也没能结束。\n"
"\\N[27]　魔物仍在四处肆虐着」"
msgstr ""

#. ID 22, Page 5, Line 22, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「看来，若是想真正解决这场异变，\n"
"\\N[27]　就非要彻底查清其中原由不可了」"
msgstr ""

#. ID 22, Page 5, Line 25, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「为此，余不断派人从古代遗迹\n"
"\\N[27]　发掘出文献，再召来学者进行\n"
"\\N[27]　翻译和研究」"
msgstr ""

#. ID 22, Page 5, Line 29, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「总有一天，异变的原因\n"
"\\N[27]　会经由余等之手被揭晓，\n"
"\\N[27]　你无须焦急」"
msgstr ""

#. ID 22, Page 5, Line 33, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「…………不过，在文献的记载中反复\n"
"\\N[27]　提到的，那个永恒的阿尔凯亚帝国，\n"
"\\N[27]　它究竟成就了何等的伟业啊」"
msgstr ""

#. ID 22, Page 5, Line 37, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「根据余等发掘出的记录，\n"
"\\N[27]  帝国的领土甚至遍布大河领域的全土。\n"
"\\N[27]  那是最伟大的英雄才能完成的霸业！」"
msgstr ""

#. ID 22, Page 5, Line 43, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「近年来，在荒野南部的内海沿岸上\n"
"\\N[27]　兴起了崇信异教的国家，他们最远\n"
"\\N[27]　甚至入侵到了大河河畔」"
msgstr ""

#. ID 22, Page 5, Line 47, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「异教徒们兵强马壮，\n"
"\\N[27]　即使神殿派出援军出面阻挡，\n"
"\\N[27]　也不过只能勉强制止他们的攻势而已」"
msgstr ""

#. ID 22, Page 5, Line 51, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「再这样下去，终有一日大河流域\n"
"\\N[27]　的各国会重蹈历史的覆辙，\n"
"\\N[27]　魔王统治世界的时代会再度来临」"
msgstr ""

#. ID 22, Page 5, Line 55, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「……但是啊，异教徒强大的秘密\n"
"\\N[27]　就在于他们只有一个信仰，一个神明。\n"
"\\N[27]　一个统一了意志的民族没有不强的道理」"
msgstr ""

#. ID 22, Page 5, Line 59, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「既然如此，只要余等民族，\n"
"\\N[27]　也将意志统一便可。\n"
"\\N[27]　为此即使有所牺牲，也在所不惜」"
msgstr ""

#. ID 22, Page 6, Line 4, Pos (11,5)
#. Choice (2 options)
msgid ""
"交谈\n"
"出售道具"
msgstr ""

#. ID 22, Page 6, Line 7, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「说起来余还没问过，\n"
"\\N[27]　你是为什么而探索遗迹的？」"
msgstr ""

#. ID 22, Page 6, Line 10, Pos (11,5)
#. Choice (3 options)
msgid ""
"为了拯救城镇\n"
"为了财宝\n"
"只是顺势而为"
msgstr ""

#. ID 22, Page 6, Line 12, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「即是为守护城镇，而与古代的怪物交战，\n"
"\\N[27]　希望能以此解决灾祸的根源吗。\n"
"\\N[27]　这是很不错的想法」"
msgstr ""

#. ID 22, Page 6, Line 16, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「但是啊，你仔细想想吧。\n"
"\\N[27]　你现在变得比以前更强了吧？\n"
"\\N[27]　得益于你在遗迹中得到的锻炼」"
msgstr ""

#. ID 22, Page 6, Line 20, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「凡天将降大任于斯人也，\n"
"\\N[27]　必先苦其心志，劳其筋骨。\n"
"\\N[27]　这场灾祸难道不是必要的试炼吗？」"
msgstr ""

#. ID 22, Page 6, Line 24, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「若是将眼光放得长远一些，\n"
"\\N[27]  这个城镇出现的牺牲，\n"
"\\N[27]　对时代来说可能也是必要之物吧？」"
msgstr ""

#. ID 22, Page 6, Line 28, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「只为故乡和亲友而战是很简单。\n"
"\\N[27]　但是像你和余这样的英雄人物，\n"
"\\N[27]　必须纵览全局，为更远大的目标而战」"
msgstr ""

#. ID 22, Page 6, Line 34, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哈哈哈，原来你也为那个\n"
"\\N[27]　遗迹里的财宝所着迷吗。\n"
"\\N[27]　其实余也一样」"
msgstr ""

#. ID 22, Page 6, Line 38, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「惟欲望方能驱使人类不断变强。\n"
"\\N[27]　身为一介生命，这也是理所当然」"
msgstr ""

#. ID 22, Page 6, Line 41, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「但是，那个遗迹真正的财宝\n"
"\\N[27]　并不是什么金银珠宝。\n"
"\\N[27]　惟独这点，你千万不可误会」"
msgstr ""

#. ID 22, Page 6, Line 45, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「存在于遥远往昔的巨大帝国……\n"
"\\N[27]　那些遗失的学问和魔术、技术……\n"
"\\N[27]　才是更胜黄金之城的宝物」"
msgstr ""

#. ID 22, Page 6, Line 51, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「哦？　没有特别的理由吗？\n"
"\\N[27]　既然如此，将你的那份力量\n"
"\\N[27]　为余所用又何妨」"
msgstr ""

#. ID 22, Page 6, Line 55, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「余终有一日，要亲手将这个\n"
"\\N[27]　涅斯公国改变成像阿尔凯亚\n"
"\\N[27]　那样的伟大国度」"
msgstr ""

#. ID 22, Page 6, Line 59, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「然后，将自己的名字深深刻在历史之上」"
msgstr ""

#. ID 22, Page 6, Line 65, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「大河神殿军的大神官，\n"
"\\N[27]　似乎要来这个镇上视察」"
msgstr ""

#. ID 22, Page 6, Line 68, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「他被人们称为＜战将巴鲁斯穆斯＞。\n"
"\\N[27]　在与异教徒的战争中打响了自己的名号」"
msgstr ""

#. ID 23, Page 4, Line 3, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「啊、\\N[\\V[2]]！\n"
"\\N[27]　能见到你真是太好了」"
msgstr ""

#. ID 23, Page 4, Line 6, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「我有事必须回首都了，\n"
"\\N[27]　想在走之前和你讲些重要的事情」"
msgstr ""

#. ID 23, Page 4, Line 9, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「……虽然让儿童石化的异变已经结束了，\n"
"\\N[27]　怪物们的扫荡也在顺利进展着，\n"
"\\N[27]　但仍然有很多问题需要解决」"
msgstr ""

#. ID 23, Page 4, Line 13, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「涅斯的大公病危。\n"
"\\N[27]　他的后继人\\N[130]，\n"
"\\N[27]　也继主要诸侯们身故后失踪……」"
msgstr ""

#. ID 23, Page 4, Line 17, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「在如今的涅斯公国，支持\\N[130]\n"
"\\N[27]　的帝国派，和想取得独立地位的\n"
"\\N[27]　诸侯派之间，战争已经一触即发了」"
msgstr ""

#. ID 23, Page 4, Line 21, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「多亏双方都没有合格的指导者，\n"
"\\N[27]　战争才没有真的被打响……」"
msgstr ""

#. ID 23, Page 4, Line 24, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「如果不早点把国内这种局面解决，\n"
"\\N[27]　西席瓦尔肯定又会来趁火打劫」"
msgstr ""

#. ID 23, Page 4, Line 27, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「所以我要回首都纳泽利去，\n"
"\\N[27]　努力试着调和两派之间的矛盾」"
msgstr ""

#. ID 23, Page 4, Line 30, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「其实\\N[130]留有一对子嗣，\n"
"\\N[27]　能好好利用那些孩子的话\n"
"\\N[27]　也许就有办法了……」"
msgstr ""

#. ID 23, Page 4, Line 34, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「……『利用』孩子什么的，感觉真阴险啊。\n"
"\\N[27]　为什么一旦想要完成有意义的大事，\n"
"\\N[27]　只靠做漂亮事总是会力有未逮…」"
msgstr ""

#. ID 23, Page 4, Line 38, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「只凭我的力量究竟能做到什么地步呢。\n"
"\\N[27]　虽然我自己也很不安，但我决定，\n"
"\\N[27]　要走上一条不迷失自己方向的道路」"
msgstr ""

#. ID 23, Page 4, Line 47, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「那个`、接下来、说回正题吧。\n"
"\\N[27]　现在我作为大公的代官，\n"
"\\N[27]　在形式上也算是接管了霍尔姆」"
msgstr ""

#. ID 23, Page 4, Line 51, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「但是，我返回首都的话，就没人\n"
"\\N[27]　指挥国境的警备和怪物的退治工作了」"
msgstr ""

#. ID 23, Page 4, Line 55, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「所以，我希望\\N[\\V[2]]能\n"
"\\N[27]　正式继承霍尔姆伯爵的称号，\n"
"\\N[27]　作为领主守护这个城镇」"
msgstr ""

#. ID 23, Page 4, Line 61, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「所以，我想授予\\N[\\V[2]]\n"
"\\N[27]　骑士的称号，将这个镇的事情托付给你」"
msgstr ""

#. ID 23, Page 4, Line 64, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「按照之前的说法，解决异变的人本来\n"
"\\N[27]　就会被给予骑士的身份，\n"
"\\N[27]　结果却因为各种混乱而变得不了了之」"
msgstr ""

#. ID 23, Page 4, Line 68, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「因此，由我负起责任来\n"
"\\N[27]　让\\N[\\V[2]]就任骑士\n"
"\\N[27]　也不会有任何问题」"
msgstr ""

#. ID 23, Page 4, Line 75, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「怎么样。\n"
"\\N[27]　你能接受吗？」"
msgstr ""

#. ID 23, Page 4, Line 84, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「太好了！\n"
"\\N[27]　我就知道你会答应的」"
msgstr ""

#. ID 23, Page 4, Line 88, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「说起来，\\N[\\V[2]]还没接受\n"
"\\N[27]　过正式的骑士受封仪式吧？\n"
"\\N[27]　那就现在一起进行吧」"
msgstr ""

#. ID 23, Page 4, Line 95, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「啊么，请你稍微蹲下身子……\n"
"\\N[27]　……对，就是这样」"
msgstr ""

#. ID 23, Page 4, Line 100, Pos (14,8)
msgid "\\N[27]\\N[11]拔出配剑，放置在你的肩膀上。"
msgstr ""

#. ID 23, Page 4, Line 103, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「咳咳……」"
msgstr ""

#. ID 23, Page 4, Line 106, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「霍尔姆的战士，\\N[\\V[2]]啊。\n"
"\\N[27]　你愿意发誓，要为守护自己的主上和信仰，\n"
"\\N[27]　为守护弱者而战吗？」"
msgstr ""

#. ID 23, Page 4, Line 117, Pos (14,8)
msgid "\\N[27]\\N[11]就地跌倒了。"
msgstr ""

#. ID 23, Page 4, Line 119, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「那、那个～、这是仪式的形式来着，\n"
"\\N[27]　总之请你先回答一下『是』」"
msgstr ""

#. ID 23, Page 4, Line 123, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「总之形式一下就行了……」"
msgstr ""

#. ID 23, Page 4, Line 127, Pos (14,8)
msgid "\\N[27]大概也就是这么回事了吧。"
msgstr ""

#. ID 23, Page 4, Line 133, Pos (14,8)
msgid ""
"\\N[27]\\N[\\V[2]]成为了公国骑士！\n"
"\\N[27]得到了霍尔姆伯爵的称号！"
msgstr ""

#. ID 23, Page 4, Line 135, Pos (14,8)
#. ChangeHeroTitle (Actor 1)
msgctxt "actors.title"
msgid "霍尔姆伯爵"
msgstr ""

#. ID 23, Page 4, Line 138, Pos (14,8)
msgid "\\N[27]\\N[\\V[2]]成为了公国骑士！"
msgstr ""

#. ID 23, Page 4, Line 139, Pos (14,8)
#. ChangeHeroTitle (Actor 2)
#. ID 23, Page 4, Line 140, Pos (14,8)
#. ChangeHeroTitle (Actor 3)
#. ID 23, Page 4, Line 141, Pos (14,8)
#. ChangeHeroTitle (Actor 4)
msgctxt "actors.title"
msgid "公国骑士"
msgstr ""

#. ID 23, Page 4, Line 153, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「……好。\n"
"\\N[27]　这样一来\\N[\\V[2]]\n"
"\\N[27]　就是一名涅斯的骑士了」"
msgstr ""

#. ID 23, Page 4, Line 157, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「因为这不是正式的仪式，\n"
"\\N[27]　以后可能会被人说三道四的……」"
msgstr ""

#. ID 23, Page 4, Line 161, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「碰到那种人，就请你说出我和我父亲的名号\n"
"\\N[27]　对他们坚称『我是公国骑士』。\n"
"\\N[27]　这种事情谁的气势大谁就赢了」"
msgstr ""

#. ID 23, Page 4, Line 167, Pos (14,8)
msgid "\\N[27]意外地很随便啊。"
msgstr ""

#. ID 23, Page 4, Line 172, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「……\\N[\\V[2]]。\n"
"\\N[27]　你对我来说，\n"
"\\N[27]　永远都是我憧憬的对象」"
msgstr ""

#. ID 23, Page 4, Line 176, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「能与你成为战友是我的荣幸」"
msgstr ""

#. ID 23, Page 4, Line 180, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「……说实话，其实我刚来这里的时候，\n"
"\\N[27]　是为了与\\N[\\V[2]]小姐结婚，控制霍尔姆领。\n"
"\\N[27]　这是\\N[130]的主意」"
msgstr ""

#. ID 23, Page 4, Line 184, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「最初我很不情愿……\n"
"\\N[27]　但当我和\\N[\\V[2]]小姐相遇之后，\n"
"\\N[27]　我却对这种命运感到欣喜……」"
msgstr ""

#. ID 23, Page 4, Line 188, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「之后，在我们作为战友一起战斗的时间里，\n"
"\\N[27]　我对你越发尊敬起来，\n"
"\\N[27]　我开始为自己原来的想法感到羞愧」"
msgstr ""

#. ID 23, Page 4, Line 193, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「但是，总有一天……我会成为配得上你\n"
"\\N[27]　的骑士，再回来见你」"
msgstr ""

#. ID 23, Page 4, Line 198, Pos (14,8)
msgid "\\N[27]\\N[11]牵起你的手，轻轻吻了一下指尖。"
msgstr ""

#. ID 23, Page 4, Line 200, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「请你一定要等我回来」"
msgstr ""

#. ID 23, Page 4, Line 211, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「……那、我要出发了。\n"
"\\N[27]　这城镇的人就交给你守护了」"
msgstr ""

#. ID 23, Page 4, Line 217, Pos (14,8)
#. ID 23, Page 4, Line 241, Pos (14,8)
msgid ""
"\\N[27]然后，\\N[11]就带着随从\n"
"\\N[27]离开了城镇……"
msgstr ""

#. ID 23, Page 4, Line 222, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「这、这样啊……\n"
"\\N[27]　真遗憾」"
msgstr ""

#. ID 23, Page 4, Line 225, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「如果你改变了想法，\n"
"\\N[27]　一定要再来跟我讲啊」"
msgstr ""

#. ID 23, Page 4, Line 232, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「要说的就是这些，那么，我要走了。\n"
"\\N[27]　这以后的事情就拜托你了」"
msgstr ""

#. ID 23, Page 4, Line 236, Pos (14,8)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「请你从灾祸之中，守护好这里的人民吧」"
msgstr ""

#. ID 27, Page 1, Line 2, Pos (5,9)
msgid ""
"\\N[27]骑士\n"
"\\N[27]「在如今这种没有战争的时代，\n"
"\\N[27]　我们这样的武人已经没有价值了吗。\n"
"\\N[27]　一直都被当作是吃干饭的」"
msgstr ""

#. ID 27, Page 1, Line 6, Pos (5,9)
msgid ""
"\\N[27]骑士\n"
"\\N[27]「但多亏了\\N[130]殿下，\n"
"\\N[27]　他让失去了昔日荣誉的我们，\n"
"\\N[27]　重新寻回了骑士的职责！」"
msgstr ""

#. ID 27, Page 4, Line 3, Pos (5,9)
msgid ""
"\\N[27]骑士\n"
"\\N[27]「不好了……\n"
"\\N[27]　要是\\N[130]大人不在的事情\n"
"\\N[27]　被人知道了……」"
msgstr ""

#. ID 27, Page 4, Line 7, Pos (5,9)
msgid "\\N[27]骑士在小声嘟囔着什么。"
msgstr ""

#. ID 27, Page 4, Line 10, Pos (5,9)
msgid ""
"\\N[27]骑士\n"
"\\N[27]「大部队已经前去保卫国境了」"
msgstr ""

#. ID 33, Page 2, Line 2, Pos (4,9)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「在下是从乡下出身\n"
"\\N[27]　参加军队的哦。\n"
"\\N[27]　请多指教哦」"
msgstr ""

