#. ID 1, Page 1, Line 5, Pos (0,0)
msgid ""
"\\N[27]※人の顔にカーソ\ルをあてて決定ボタンを押すと、\n"
"\\N[27]　相手に話しかけることができます。"
msgstr ""

#. ID 2, Page 1, Line 21, Pos (0,1)
msgid "\\N[27]その夜―\―\。"
msgstr ""

#. ID 2, Page 1, Line 23, Pos (0,1)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「\\N[\\V[2]]さま……\n"
"\\N[27]　\\N[\\V[2]]さま、起きてください」"
msgstr ""

#. ID 2, Page 1, Line 26, Pos (0,1)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「起こしてしまい申\し訳ありません。\n"
"\\N[27]　町で、異変が起きているらしいのです」"
msgstr ""

#. ID 2, Page 1, Line 33, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「わからん、落ち着いて話せ。\n"
"\\N[27]　何が起きている？」"
msgstr ""

#. ID 2, Page 1, Line 37, Pos (0,1)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「ですから、怪物です。\n"
"\\N[27]　背の低い、醜い子供のようなモノが、\n"
"\\N[27]　武器を持って町を襲っているのです」"
msgstr ""

#. ID 2, Page 1, Line 41, Pos (0,1)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「力も強く、仲間が二人やられました。\n"
"\\N[27]　あれはまるで……おとぎ話に出てくる、\n"
"\\N[27]　小鬼のような……」"
msgstr ""

#. ID 2, Page 1, Line 46, Pos (0,1)
msgid ""
"\\N[27]ゼペック\n"
"\\N[27]「怪物どもは相当な数いるようですな。\n"
"\\N[27]　あちこちの家を襲撃しています」"
msgstr ""

#. ID 2, Page 1, Line 50, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「怪物だろうと、生身の手足を持つなら\n"
"\\N[27]　野盗の群れと変わらぬ」"
msgstr ""

#. ID 2, Page 1, Line 53, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「国境警備隊に伝令を出せ！\n"
"\\N[27]　ゼペック、兵を率いて出ろ。\n"
"\\N[27]　自警団を招集しつつ、敵を掃討せよ」"
msgstr ""

#. ID 2, Page 1, Line 58, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[\\V[2]]。お前は町を回り、\n"
"\\N[27]　襲われている者を助けよ！」"
msgstr ""

#. ID 2, Page 1, Line 63, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[\\V[2]]。お前は危険の無いよう、\n"
"\\N[27]　安全な場所に隠れていなさい！」"
msgstr ""

#. ID 2, Page 2, Line 6, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「おお、丁度良い。\n"
"\\N[27]　\\N[\\V[2]]、お前に会わせたい方が\n"
"\\N[27]　来ておられる」"
msgstr ""

#. ID 2, Page 2, Line 13, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「はじめまして、\\N[\\V[2]]さん。\n"
"\\N[27]　お会いできて光栄です」"
msgstr ""

#. ID 2, Page 2, Line 17, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「コホン。あー、\\N[11]殿は\n"
"\\N[27]　大公陛下の血筋に連なる名家の方でな。\n"
"\\N[27]　公国救済のため、探索に参加される」"
msgstr ""

#. ID 2, Page 2, Line 22, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「そうなんですよ。僕の領地でも大変な\n"
"\\N[27]　事になってて。民が困ってたら\n"
"\\N[27]　頑張るのが騎士のつとめですから」"
msgstr ""

#. ID 2, Page 2, Line 26, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「ヘンな怪物が出るらしいですが、\n"
"\\N[27]　そんなのデリャー、トリャーってな\n"
"\\N[27]　もんですよ！」"
msgstr ""

#. ID 2, Page 2, Line 30, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「\\N[\\V[2]]さんも探索に参加するん\n"
"\\N[27]　でしょう？　一緒に戦いましょう！」"
msgstr ""

#. ID 2, Page 2, Line 34, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「コホン。えーと、ここで\\N[11]殿と\n"
"\\N[27]　友誼を持つことは、我が家にとって\n"
"\\N[27]　たいへん名誉なことだ」"
msgstr ""

#. ID 2, Page 2, Line 38, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「いくさ場で交わした友情は、社交で\n"
"\\N[27]　十\年培ったそれに勝る。\n"
"\\N[27]　何かと協力して差し上げると良い」"
msgstr ""

#. ID 2, Page 2, Line 43, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「僕も頑張ります！」"
msgstr ""

#. ID 2, Page 2, Line 45, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「それじゃ一足お先に、酒場のあたりで\n"
"\\N[27]　他の仲間を探してきます。\n"
"\\N[27]　また会いましょうっ」"
msgstr ""

#. ID 2, Page 2, Line 51, Pos (0,1)
#. ID 2, Page 3, Line 138, Pos (0,1)
#. ID 2, Page 4, Line 270, Pos (0,1)
#. ID 4, Page 8, Line 47, Pos (15,7)
#. ID 4, Page 8, Line 85, Pos (15,7)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「…………」"
msgstr ""

#. ID 2, Page 2, Line 53, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「ま、まあ、\\N[\\V[2]]にも苦労を\n"
"\\N[27]　かけるかも知れんが。\n"
"\\N[27]　彼のことを頼むぞ」"
msgstr ""

#. ID 2, Page 2, Line 60, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「はじめまして、\\N[\\V[2]]さん。\n"
"\\N[27]　いやあ、美しい方ですね。\n"
"\\N[27]　お会いできて光栄です！」"
msgstr ""

#. ID 2, Page 2, Line 65, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「ハッハッハ。\\N[11]殿は\n"
"\\N[27]　大公陛下の血筋に連なる名家の方でな。\n"
"\\N[27]　公国救済のため、探索に参加される」"
msgstr ""

#. ID 2, Page 2, Line 69, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「ここで\\N[11]殿のお家と繋がりを\n"
"\\N[27]　持つことは、我が家としてもたいへん\n"
"\\N[27]　名誉なことだ。ハッハッハ」"
msgstr ""

#. ID 2, Page 2, Line 74, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「僕もとても嬉しいです。\n"
"\\N[27]　こんな美しい方との縁談だなんて」"
msgstr ""

#. ID 2, Page 2, Line 78, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「ハッハッハ。いや、それはまだ決定\n"
"\\N[27]　していない話だが、もし実現したら\n"
"\\N[27]　双方の家にとって良い話ですな」"
msgstr ""

#. ID 2, Page 2, Line 82, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「ただまあ、その、この娘はとんだ\n"
"\\N[27]　おてんばで、お気に召さないかも\n"
"\\N[27]　しれなくてですな。ハッハッハ」"
msgstr ""

#. ID 2, Page 2, Line 87, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「いやあ、僕はそんなの気になりません。\n"
"\\N[27]　強い女性は好きです。一緒に探索して、\n"
"\\N[27]　怪物たちをやっつけましょう」"
msgstr ""

#. ID 2, Page 2, Line 91, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「民を苦しみから救うため、共に頑張り\n"
"\\N[27]　ましょう！\n"
"\\N[27]　愛は公国を救う。なんちゃって」"
msgstr ""

#. ID 2, Page 2, Line 96, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「ハッハッハ。調子に乗るなよ小僧」"
msgstr ""

#. ID 2, Page 2, Line 99, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「はい？」"
msgstr ""

#. ID 2, Page 2, Line 102, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「いや、何でもありません。ハッハッハ」"
msgstr ""

#. ID 2, Page 2, Line 105, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「そうですか。\n"
"\\N[27]　それじゃ、一足お先に、酒場のあたりで\n"
"\\N[27]　他の仲間を探してきます」"
msgstr ""

#. ID 2, Page 2, Line 109, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「またお会いましょう！」"
msgstr ""

#. ID 2, Page 2, Line 113, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「……………………」"
msgstr ""

#. ID 2, Page 2, Line 115, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「………………すまん。\n"
"\\N[27]　無理はしなくて良いからな…………」"
msgstr ""

#. ID 2, Page 3, Line 7, Pos (0,1)
msgid "\\N[27]館に入ると、雰囲気がいつもと違うことに気付いた。"
msgstr ""

#. ID 2, Page 3, Line 9, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「……帰ってきたか」"
msgstr ""

#. ID 2, Page 3, Line 12, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[130]殿下。\n"
"\\N[27]　あそこにいるのが私の息子、\n"
"\\N[27]　\\N[\\V[2]]です」"
msgstr ""

#. ID 2, Page 3, Line 18, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[130]殿下。\n"
"\\N[27]　あそこにいるのが私の娘、\n"
"\\N[27]　\\N[\\V[2]]です」"
msgstr ""

#. ID 2, Page 3, Line 25, Pos (0,1)
#. ID 2, Page 3, Line 64, Pos (0,1)
msgid ""
"\\N[27]階段の上に見慣れない人影があった。\n"
"\\N[27]美髯をたくわえ、甲冑を着た貴公子が、\n"
"\\N[27]気怠げに階段の手すりに寄りかかりながら、\n"
"\\N[27]こちらを見下ろしている。"
msgstr ""

#. ID 2, Page 3, Line 31, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「おや……。\n"
"\\N[27]　また会ったな」"
msgstr ""

#. ID 2, Page 3, Line 36, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「ほう、貴公が……」"
msgstr ""

#. ID 2, Page 3, Line 38, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「己(おれ)はネスの\\N[130]。\n"
"\\N[27]　ネス公国大公の息子だ」"
msgstr ""

#. ID 2, Page 3, Line 42, Pos (0,1)
#. ID 2, Page 3, Line 74, Pos (0,1)
msgid "\\N[27]貴公子の顔には、深い十\文字傷が走っている。"
msgstr ""

#. ID 2, Page 3, Line 47, Pos (0,1)
msgid "\\N[27]館に入ろうとしたところで、執事に止められた。"
msgstr ""

#. ID 2, Page 3, Line 49, Pos (0,1)
msgid ""
"\\N[27]ゼペック\n"
"\\N[27]「今は客人が来ていて、\n"
"\\N[27]　お前たちの相手ができんのだ。\n"
"\\N[27]　すまんが今日は帰ってくれ」"
msgstr ""

#. ID 2, Page 3, Line 54, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「いや、かまわん。\n"
"\\N[27]　遺跡で怪物と戦う探索者というのと\n"
"\\N[27]　直接話をしてみたい」"
msgstr ""

#. ID 2, Page 3, Line 59, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「それに、知らぬ相手でもないしな」"
msgstr ""

#. ID 2, Page 3, Line 69, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「己(おれ)はネスの\\N[130]。\n"
"\\N[27]　ネス公国大公の息子だ。\n"
"\\N[27]　この遺跡の様子を見に来た」"
msgstr ""

#. ID 2, Page 3, Line 82, Pos (0,1)
#. ID 22, Page 1, Line 4, Pos (11,5)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「お久しぶりです、\\N[130]。\n"
"\\N[27]　あなたもこっちに来てたんですか」"
msgstr ""

#. ID 2, Page 3, Line 86, Pos (0,1)
#. ID 22, Page 1, Line 8, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「おう、\\N[11]か」"
msgstr ""

#. ID 2, Page 3, Line 88, Pos (0,1)
#. ID 22, Page 1, Line 10, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「変わらず元気そうで何よりだ。\n"
"\\N[27]　大過なく役目を果たしているか？」"
msgstr ""

#. ID 2, Page 3, Line 92, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「ええ。公国のために\n"
"\\N[27]　毎日ガンバってますよ」"
msgstr ""

#. ID 2, Page 3, Line 95, Pos (0,1)
#. ID 22, Page 1, Line 18, Pos (11,5)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「あ、\\N[130]公子は僕の従兄弟です」"
msgstr ""

#. ID 2, Page 3, Line 97, Pos (0,1)
#. ID 22, Page 1, Line 20, Pos (11,5)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「ちょっと変わり者なので、誤解される\n"
"\\N[27]　事もあるけど、とても立派な考えを\n"
"\\N[27]　持ってて、僕の憧れの人なんですよ」"
msgstr ""

#. ID 2, Page 3, Line 102, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「ハハハ、おだてるな」"
msgstr ""

#. ID 2, Page 3, Line 106, Pos (0,1)
#. ID 22, Page 1, Line 29, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「ところで、\\N[\\V[2]]殿」"
msgstr ""

#. ID 2, Page 3, Line 108, Pos (0,1)
#. ID 22, Page 1, Line 31, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「確か\\N[11]との\n"
"\\N[27]　見合い話があったはずだが。\n"
"\\N[27]　どうだ、貴公にその気はあるか？」"
msgstr ""

#. ID 2, Page 3, Line 113, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「て、\\N[130]、\n"
"\\N[27]　急にそんな話は……」"
msgstr ""

#. ID 2, Page 3, Line 117, Pos (0,1)
#. ID 22, Page 1, Line 39, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「ハハハッ。貴公との縁談を\n"
"\\N[27]　\\N[11]に勧めたのは己だ。\n"
"\\N[27]　まとまって貰わねば己の顔が立たん」"
msgstr ""

#. ID 2, Page 3, Line 122, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「その件は、実に有難い話ですが……。\n"
"\\N[27]　当人たちにまかせるのが良いかと\n"
"\\N[27]　私は考えております」"
msgstr ""

#. ID 2, Page 3, Line 127, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「ハハッ、ここは若い者にまかせて、\n"
"\\N[27]　という訳か」"
msgstr ""

#. ID 2, Page 3, Line 130, Pos (0,1)
#. ID 22, Page 1, Line 43, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「……いずれ、大公家と有力諸侯が\n"
"\\N[27]　力を一つにすべき時代が来る」"
msgstr ""

#. ID 2, Page 3, Line 133, Pos (0,1)
#. ID 22, Page 1, Line 46, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「そのためには婚姻が大事だ。\n"
"\\N[27]　惚れあった男女の仲ほど裂きがたい\n"
"\\N[27]　ものは無いからな。ハッハッハ！」"
msgstr ""

#. ID 2, Page 3, Line 141, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「だがそれも、この騒動を終わらせて\n"
"\\N[27]　からの事―\―\」"
msgstr ""

#. ID 2, Page 3, Line 155, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「己がこちらに来たのは、\n"
"\\N[27]　遺跡探索を直に監督するためだ」"
msgstr ""

#. ID 2, Page 3, Line 158, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「……遺跡の災厄が起きてから、\n"
"\\N[27]　各地の諸侯が動揺している。\n"
"\\N[27]　さらに他国の動きも気になるところだ」"
msgstr ""

#. ID 2, Page 3, Line 162, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「我がネス公国は諸侯領による連邦国家。\n"
"\\N[27]　外敵に狙われた時に結束が緩んでいては\n"
"\\N[27]　国そのものが危うい」"
msgstr ""

#. ID 2, Page 3, Line 166, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「それこそが真の災厄だ。\n"
"\\N[27]　これを避けるために、探索を進め、\n"
"\\N[27]　怪異を終わらせねばならぬ」"
msgstr ""

#. ID 2, Page 3, Line 170, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「己も配下の＜火車騎士団＞を使って\n"
"\\N[27]　ここの周囲の化け物どもを退治しよう。\n"
"\\N[27]　お前たちも、力を尽くせ」"
msgstr ""

#. ID 2, Page 4, Line 7, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[\\V[2]]か。\n"
"\\N[27]　帰ってくるのを待っていたぞ」"
msgstr ""

#. ID 2, Page 4, Line 12, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[\\V[2]]。\n"
"\\N[27]　少し待ちなさい」"
msgstr ""

#. ID 2, Page 4, Line 17, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「今夜、来客がある。\n"
"\\N[27]　ユールフレール島の大河神殿から来た\n"
"\\N[27]　バルスムス大神官だ」"
msgstr ""

#. ID 2, Page 4, Line 21, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「彼の通称は＜戦将バルスムス＞。\n"
"\\N[27]　聖戦士たちを率いて異教徒と戦い、\n"
"\\N[27]　多くの戦功を挙げたお人だ」"
msgstr ""

#. ID 2, Page 4, Line 26, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「……あの大神官殿は、この町で起きた\n"
"\\N[27]　異変の状況を確認するために来た、\n"
"\\N[27]　と主張している」"
msgstr ""

#. ID 2, Page 4, Line 30, Pos (0,1)
#. ID 22, Page 6, Line 71, Pos (11,5)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「だが、ただの視察のためだけに\n"
"\\N[27]　＜戦将＞などと大層な名を持つ武闘派が\n"
"\\N[27]　送り込まれてくるはずがない」"
msgstr ""

#. ID 2, Page 4, Line 35, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「今日の晩餐は彼と共にする事になる。\n"
"\\N[27]　\\N[\\V[2]]、お前も出席せよ」"
msgstr ""

#. ID 2, Page 4, Line 40, Pos (0,1)
#. Choice (2 options)
msgid ""
"今日はまだ用事がある\n"
"晩餐に出席する"
msgstr ""

#. ID 2, Page 4, Line 43, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「そうか。\n"
"\\N[27]　では、用事が済んだら\n"
"\\N[27]　すぐ館に戻れ」"
msgstr ""

#. ID 2, Page 4, Line 58, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「では、夜まで待機しておきなさい」"
msgstr ""

#. ID 2, Page 4, Line 64, Pos (0,1)
msgid ""
"\\N[27]日が暮れた頃、食事の支度が整えられ、\n"
"\\N[27]あなたもその席に呼ばれた。"
msgstr ""

#. ID 2, Page 4, Line 72, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「おや、貴殿は……。\n"
"\\N[27]　先ほどもお会いしましたな」"
msgstr ""

#. ID 2, Page 4, Line 76, Pos (0,1)
msgid ""
"\\N[27]紹介されたのは、昼間に言葉を交わした人物だった。\n"
"\n"
"\\N[27]互いに挨拶する。"
msgstr ""

#. ID 2, Page 4, Line 82, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「急な訪問であったというのに、\n"
"\\N[27]　このようにもてなして頂き、\n"
"\\N[27]　恐縮です」"
msgstr ""

#. ID 2, Page 4, Line 87, Pos (0,1)
msgid ""
"\\N[27]客は、禿頭だが逞しい体格の老神官である。\n"
"\\N[27]顔立ちは凶暴\、額に刀傷が走り、耳は潰れている。\n"
"\\N[27]きわめつきの悪相だが、目や口調からは\n"
"\\N[27]誠実そうな人柄が伝わってくる。"
msgstr ""

#. ID 2, Page 4, Line 92, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「ハハハ、遠慮することはない」"
msgstr ""

#. ID 2, Page 4, Line 95, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「ええ。名高い大神官バルスムス殿を\n"
"\\N[27]　お迎えすることは、\n"
"\\N[27]　我らにとって光栄なことです」"
msgstr ""

#. ID 2, Page 4, Line 99, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「長旅でお疲れでしょう。\n"
"\\N[27]　我が家のようにおくつろぎ頂きたい」"
msgstr ""

#. ID 2, Page 4, Line 103, Pos (0,1)
msgid ""
"\\N[27]カムールと\\N[130]、バルスムスたちとあなたは、\n"
"\\N[27]食事をしながら会話を続けた。\n"
"\\N[27]ふと気付くと意味ありげな沈黙が漂っているような、\n"
"\\N[27]緊張感のある食卓だった。"
msgstr ""

#. ID 2, Page 4, Line 108, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「―\―\南方の荒野地方で続いていた、\n"
"\\N[27]　エルパディアと異教徒たちの戦いは、\n"
"\\N[27]　一時小休止となったようです」"
msgstr ""

#. ID 2, Page 4, Line 112, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「拙僧らも幾度か援軍に赴きましたが、\n"
"\\N[27]　戦況は厳しく、休戦までの間に\n"
"\\N[27]　河南の都市がいくつも奪われました」"
msgstr ""

#. ID 2, Page 4, Line 116, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「僧兵の役目は、大河の神々を奉じる\n"
"\\N[27]　国々を守ること。\n"
"\\N[27]　役目を果たせず、忸怩たる思いです」"
msgstr ""

#. ID 2, Page 4, Line 121, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「いや、バルスムス殿の働きがなければ\n"
"\\N[27]　被害はもっと大きかったことでしょう。\n"
"\\N[27]　活躍の噂\はこの僻地まで届きましたぞ」"
msgstr ""

#. ID 2, Page 4, Line 126, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「そう言って頂けると、救われます」"
msgstr ""

#. ID 2, Page 4, Line 128, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「しかし、この町は意外に平穏ですね。\n"
"\\N[27]　怪物の出現や農地への被害で、\n"
"\\N[27]　治安が悪化したかと思いましたが」"
msgstr ""

#. ID 2, Page 4, Line 133, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「いや、平穏とはなかなか行きませぬ。\n"
"\\N[27]　\\N[130]殿下の騎士団や、都からの\n"
"\\N[27]　支援に助けられている状況です」"
msgstr ""

#. ID 2, Page 4, Line 137, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「遺跡の調査が進み、災厄の原因を\n"
"\\N[27]　排除できれば良いのですが。\n"
"\\N[27]　それもまだ成果を出せずにいます」"
msgstr ""

#. ID 2, Page 4, Line 142, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「なるほど。\n"
"\n"
"\\N[27]　……カムール卿、提案なのですが」"
msgstr ""

#. ID 2, Page 4, Line 146, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「その遺跡の管理と調査、\n"
"\\N[27]　我々大河神殿が行いましょうか？」"
msgstr ""

#. ID 2, Page 4, Line 150, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「と、申\しますと……」"
msgstr ""

#. ID 2, Page 4, Line 153, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「無頼の探索者たちに任せていては、\n"
"\\N[27]　手がかりを見落とす事もあるでしょう」"
msgstr ""

#. ID 2, Page 4, Line 156, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「宝の盗掘だけが目当ての輩に頼っては、\n"
"\\N[27]　調査が進まないのも当然です」"
msgstr ""

#. ID 2, Page 4, Line 159, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「だが神殿ならば、知識のある学僧や\n"
"\\N[27]　歴戦の聖戦士たちをお貸しできます。\n"
"\\N[27]　ここは我々にお任せ頂きたい」"
msgstr ""

#. ID 2, Page 4, Line 165, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「ハハハ……。\n"
"\\N[27]　それが目的であったか」"
msgstr ""

#. ID 2, Page 4, Line 169, Pos (0,1)
msgid "\\N[27]沈黙していた\\N[130]が、唐突に笑い出した。"
msgstr ""

#. ID 2, Page 4, Line 171, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[130]殿下……？」"
msgstr ""

#. ID 2, Page 4, Line 174, Pos (0,1)
#. ID 2, Page 4, Line 218, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「…………」"
msgstr ""

#. ID 2, Page 4, Line 177, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「貴公は要するに、遺跡で手に入る\n"
"\\N[27]　古代の知恵を、我々が独占しているのが\n"
"\\N[27]　気に入らぬのだろう？」"
msgstr ""

#. ID 2, Page 4, Line 181, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「欲しいなら欲しいと、\n"
"\\N[27]　そう言えば良いのだ」"
msgstr ""

#. ID 2, Page 4, Line 185, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「…………違います」"
msgstr ""

#. ID 2, Page 4, Line 187, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「貴殿が古代の知恵と呼ぶものは、\n"
"\\N[27]　危険なものなのです」"
msgstr ""

#. ID 2, Page 4, Line 190, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「かつてアルケア帝国の皇帝たちは、\n"
"\\N[27]　世界を支配しながらも、\n"
"\\N[27]　慢心によって神々に滅ぼされました」"
msgstr ""

#. ID 2, Page 4, Line 194, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「彼らは、人の身でありながら\n"
"\\N[27]　人を越えようとし、ついには\n"
"\\N[27]　神々の領域を侵してしまった」"
msgstr ""

#. ID 2, Page 4, Line 198, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「あの時代の思想を現代の人々が知れば、\n"
"\\N[27]　やがて遠い将来に同じ事を繰り返し、\n"
"\\N[27]　同じように神々に罰せられるでしょう」"
msgstr ""

#. ID 2, Page 4, Line 202, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「貴殿らは、あの時代の文献を\n"
"\\N[27]　影響も考えずに拡散させている。\n"
"\\N[27]　我々はこれを止めねばなりません」"
msgstr ""

#. ID 2, Page 4, Line 207, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「そうか」"
msgstr ""

#. ID 2, Page 4, Line 210, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「御理解頂けましたか」"
msgstr ""

#. ID 2, Page 4, Line 213, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「下らん。\n"
"\\N[27]　心底下らん。\n"
"\\N[27]　例えようもなく下らん」"
msgstr ""

#. ID 2, Page 4, Line 221, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「貴公らは、\n"
"\\N[27]　神殿の権威が脅かされるのを\n"
"\\N[27]　恐れているだけだ」"
msgstr ""

#. ID 2, Page 4, Line 225, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「人が人を越え、\n"
"\\N[27]　神に至らんとする思想。\n"
"\\N[27]　素晴らしいではないか」"
msgstr ""

#. ID 2, Page 4, Line 229, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「かつて人は、偉大な種族だった。\n"
"\\N[27]　これを広く伝える事の、どこが悪い？」"
msgstr ""

#. ID 2, Page 4, Line 232, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「高みを目指すのは人間の性。\n"
"\\N[27]　頭を押さえつけようとして、\n"
"\\N[27]　押さえられるものではないぞ」"
msgstr ""

#. ID 2, Page 4, Line 237, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「……貴殿は、自分のしている事が\n"
"\\N[27]　どのような事なのか、\n"
"\\N[27]　すでに自覚していたのですね」"
msgstr ""

#. ID 2, Page 4, Line 242, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「無論」"
msgstr ""

#. ID 2, Page 4, Line 245, Pos (0,1)
msgid ""
"\\N[27]バルスムス\n"
"\\N[27]「では、これ以上の対話も説得も無意味。\n"
"\\N[27]　失礼ながら、これで席を辞することに\n"
"\\N[27]　いたしましょう」"
msgstr ""

#. ID 2, Page 4, Line 251, Pos (0,1)
msgid ""
"\\N[27]老神官は立ち上がると、少数の供を連れて\n"
"\\N[27]夜闇の中へ去っていった。"
msgstr ""

#. ID 2, Page 4, Line 254, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「食えん坊主だ。\n"
"\\N[27]　お為ごかしの口車で\n"
"\\N[27]　遺跡を奪いに来るとは」"
msgstr ""

#. ID 2, Page 4, Line 258, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「アルケア帝国についての記録があまり\n"
"\\N[27]　残っていなかったのは、あの調子で\n"
"\\N[27]　大河神殿が秘匿していたからだろうな」"
msgstr ""

#. ID 2, Page 4, Line 263, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[130]殿下……。\n"
"\\N[27]　なぜ神殿を敵に回すようなことを？」"
msgstr ""

#. ID 2, Page 4, Line 267, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「敵に回して、何か不都合でも？」"
msgstr ""

#. ID 2, Page 4, Line 273, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「そう難しい顔をするな。\n"
"\\N[27]　あの坊主は、自分たちが手綱を握れぬ\n"
"\\N[27]　ものがあるのが我慢できないだけだ」"
msgstr ""

#. ID 2, Page 4, Line 277, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「仮に奴の申\し出を受けたとしたら、\n"
"\\N[27]　遺跡もこの町も乗っ取られていた」"
msgstr ""

#. ID 2, Page 4, Line 280, Pos (0,1)
msgid ""
"\\N[27]\\N[130]\n"
"\\N[27]「奴らは別に、この町や住人を\n"
"\\N[27]　守ろうとしている訳では\n"
"\\N[27]　ないのだからな」"
msgstr ""

#. ID 2, Page 4, Line 285, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]（―\―\ですが、\\N[130]殿下。\n"
"\\N[27]　それは、貴方も同じでは\n"
"\\N[27]　ないのですか―\―\？）"
msgstr ""

#. ID 2, Page 4, Line 290, Pos (0,1)
msgid ""
"\\N[27]カムールは、言葉を飲み込んだかのような表\情で\n"
"\\N[27]沈黙している―\―\。"
msgstr ""

#. ID 2, Page 4, Line 300, Pos (0,1)
#. ID 2, Page 5, Line 211, Pos (0,1)
#. ID 3, Page 4, Line 50, Pos (16,13)
msgid "\\>\\V[31]日目　王国歴\\V[32]年　\\V[33]月　\\V[34]日\\<"
msgstr ""

#. ID 2, Page 4, Line 305, Pos (0,1)
msgid ""
"\\N[27]ゼペック\n"
"\\N[27]「悪いが、来客の予\定がある。\n"
"\\N[27]　ユールフレール島の大河神殿から来た\n"
"\\N[27]　バルスムス大神官というお人だ」"
msgstr ""

#. ID 2, Page 4, Line 309, Pos (0,1)
msgid ""
"\\N[27]ゼペック\n"
"\\N[27]「そんなわけで、すまないが、\n"
"\\N[27]　しばらく中には入れられん」"
msgstr ""

#. ID 2, Page 4, Line 318, Pos (0,1)
msgid "\\N[27]追い返されてしまった……。"
msgstr ""

#. ID 2, Page 5, Line 23, Pos (0,1)
msgid ""
"\\N[27]\\N[13]\n"
"\\N[27]「……\\N[\\V[2]]さま、\n"
"\\N[27]　目を覚ましてください。\n"
"\\N[27]　広間に来るようにと、お館様が……」"
msgstr ""

#. ID 2, Page 5, Line 29, Pos (0,1)
msgid ""
"\\N[27]朝、いつもより早く目覚めると、\n"
"\\N[27]周囲が慌ただしかった。\n"
"\\N[27]ぴりぴりと緊張感が漂っている。"
msgstr ""

#. ID 2, Page 5, Line 33, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「あ、\\N[\\V[2]]さん……。\n"
"\\N[27]　大変です。\n"
"\\N[27]　どうやら戦争になるみたいですよ」"
msgstr ""

#. ID 2, Page 5, Line 38, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「夜明け前、西の国境が侵された。\n"
"\\N[27]　敵は―\―\隣国の、西シーウァ王国」"
msgstr ""

#. ID 2, Page 5, Line 41, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「三十\年間続いた休戦を、なぜ突然破り、\n"
"\\N[27]　奇襲を仕掛けてきたのか……」"
msgstr ""

#. ID 2, Page 5, Line 44, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「現在は国境警備隊が応戦し、\n"
"\\N[27]　知らせを受けてすぐ\\N[130]殿下の\n"
"\\N[27]　騎士団が援軍に出た」"
msgstr ""

#. ID 2, Page 5, Line 48, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「我々はここで居残りだ。\n"
"\\N[27]　\\N[\\V[2]]も、町の守りにつけ」"
msgstr ""

#. ID 2, Page 5, Line 60, Pos (0,1)
msgid ""
"\\N[27]その時、神殿の鐘楼が鐘を打ち鳴らす音が\n"
"\\N[27]聞こえてきた。\n"
"\\N[27]危険を知らせる音だ。"
msgstr ""

#. ID 2, Page 5, Line 63, Pos (0,1)
msgid "\\N[27]町の警邏兵が館に駆け込んでくる。"
msgstr ""

#. ID 2, Page 5, Line 64, Pos (0,1)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「て―\―\敵襲！　敵襲です！\n"
"\\N[27]　町の西と港に敵の船が上陸しました！」"
msgstr ""

#. ID 2, Page 5, Line 68, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「なんと！\n"
"\\N[27]　シーウァめ……」"
msgstr ""

#. ID 2, Page 5, Line 72, Pos (0,1)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「いえ、それが……」"
msgstr ""

#. ID 2, Page 5, Line 74, Pos (0,1)
msgid ""
"\\N[27]兵士\n"
"\\N[27]「敵の旗印は―\―\神殿軍！\n"
"\\N[27]　大河神殿の聖戦士団が、\n"
"\\N[27]　シーウァと共に襲撃してきました！」"
msgstr ""

#. ID 2, Page 5, Line 83, Pos (0,1)
msgid ""
"\\N[27]外に出て見渡すと、町の西と南から煙が上がっている。\n"
"\\N[27]見慣れぬ鎧兜を着た敵兵たちが、町の大通りを\n"
"\\N[27]駆ける姿さえ見える。"
msgstr ""

#. ID 2, Page 5, Line 87, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「これは……」"
msgstr ""

#. ID 2, Page 5, Line 89, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「……まさか神殿が、ここまでするとは。\n"
"\\N[27]　町にいる警邏隊で抗える数ではないな\n"
"\\N[27]　……」"
msgstr ""

#. ID 2, Page 5, Line 94, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「だがそれでも、我々は騎士として\n"
"\\N[27]　戦わねばならん」"
msgstr ""

#. ID 2, Page 5, Line 100, Pos (0,1)
msgid ""
"\\N[27]―\―\やがて町の守備隊は打ち破られ、\n"
"\\N[27]敵兵たちが城館の入り口へと殺到してくる！"
msgstr ""

#. ID 2, Page 5, Line 107, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「くっ……！」"
msgstr ""

#. ID 2, Page 5, Line 110, Pos (0,1)
msgid ""
"\\N[27]目の前の敵を倒しても、すぐに新手が現れ、\n"
"\\N[27]数で圧倒されてしまう。\n"
"\\N[27]館の陥落は時間の問題だった。"
msgstr ""

#. ID 2, Page 5, Line 114, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「ここまでか……」"
msgstr ""

#. ID 2, Page 5, Line 116, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「……\\N[\\V[2]]。\n"
"\\N[27]　この館の地下にある通路から\n"
"\\N[27]　外に逃れろ」"
msgstr ""

#. ID 2, Page 5, Line 120, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[11]殿と\\N[13]と共に\n"
"\\N[27]　身を隠し、援軍を待て」"
msgstr ""

#. ID 2, Page 5, Line 124, Pos (0,1)
msgid ""
"\\N[27]逃げろと言われて戸惑っていると、\n"
"\\N[27]カムールは声を荒げた。"
msgstr ""

#. ID 2, Page 5, Line 127, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「早く行け、馬鹿者！\n"
"\\N[27]　\\N[11]殿や\\N[13]まで、\n"
"\\N[27]　ここで死なせる気か！」"
msgstr ""

#. ID 2, Page 5, Line 131, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「しばしの別れだ、我が子よ。\n"
"\n"
"\\N[27]　行け！」"
msgstr ""

#. ID 2, Page 5, Line 136, Pos (0,1)
msgid ""
"\\N[27]父親の声にはじかれるようにして、\n"
"\\N[27]あなたたちは館から逃れた……。"
msgstr ""

#. ID 2, Page 5, Line 140, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「……\\N[\\V[2]]、聞け」"
msgstr ""

#. ID 2, Page 5, Line 142, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「お前は逃れて身を隠し、\n"
"\\N[27]　救援が来るのを待て」"
msgstr ""

#. ID 2, Page 5, Line 145, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「それがお前の役目だ。\n"
"\\N[27]　分かったな……？」"
msgstr ""

#. ID 2, Page 5, Line 148, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[11]殿と\\N[13]も、\n"
"\\N[27]　\\N[\\V[2]]と共に援軍を待て」"
msgstr ""

#. ID 2, Page 5, Line 152, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「えっ！？\n"
"\\N[27]　でも、敵を目の前にして\n"
"\\N[27]　戦わずにいるなんて、騎士として……」"
msgstr ""

#. ID 2, Page 5, Line 157, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[11]殿。\n"
"\\N[27]　これは、必要な事なのだ」"
msgstr ""

#. ID 2, Page 5, Line 160, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「\\N[\\V[2]]を、お頼みします」"
msgstr ""

#. ID 2, Page 5, Line 163, Pos (0,1)
msgid ""
"\\N[27]\\N[11]\n"
"\\N[27]「…………。\n"
"\\N[27]　はい……」"
msgstr ""

#. ID 2, Page 5, Line 167, Pos (0,1)
msgid ""
"\\N[27]カムール\n"
"\\N[27]「では、しばしの別れだ、我が子よ。\n"
"\n"
"\\N[27]　行け！」"
msgstr ""

#. ID 2, Page 5, Line 172, Pos (0,1)
msgid "\\N[27]あなたは館から出て、森に身を隠そうと走る。"
msgstr ""

#. ID 2, Page 5, Line 174, Pos (0,1)
msgid "\\N[27]だが、獲物を求めていた敵兵と遭遇してしまった！"
msgstr ""

#. ID 2, Page 5, Line 179, Pos (0,1)
msgid "\\N[27]追ってきた敵兵を殴り倒し、森に逃げ込む……。"
msgstr ""

#. ID 2, Page 5, Line 182, Pos (0,1)
msgid "\\N[27]敵兵の手から逃げ延びた……。"
msgstr ""

#. ID 2, Page 5, Line 192, Pos (0,1)
msgid ""
"\\N[27]神殿軍は領主の館を攻め落とし、町を占領した。\n"
"\\N[27]そのすぐ後、西シーウァの軍隊もやってきて、\n"
"\\N[27]数日間混乱が続いた。"
msgstr ""

#. ID 2, Page 5, Line 195, Pos (0,1)
msgid ""
"\\N[27]あなたは状況が落ちつくのを待ってから、\n"
"\\N[27]港近くの下町に身を隠し、公国軍が救援に来るのを\n"
"\\N[27]待つことにした。"
msgstr ""

#. ID 3, Page 4, Line 4, Pos (16,13)
#. Contains choice at line 2 (2 options)
msgid "\\N[27]眠りに就いて、この物語を終えますか？"
msgstr ""

